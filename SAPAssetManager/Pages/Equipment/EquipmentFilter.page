{
    "OnActivityBackPressed": "/SAPAssetManager/Rules/WorkOrders/CreateUpdate/CancelCheckOnBackNavAndroid.js",
    "ActionBar": {
        "_Name": "EquipmentFilter",
        "Items": [
            {
                "Position": "Left",
                "_Name": "Cancel",
                "SystemItem": "Cancel",
                "Caption": "$(L,cancel)",
                "OnPress": "/SAPAssetManager/Actions/Page/CancelPage.action"
            },
            {
                "Position": "Right",
                "_Name": "Filter",
                "SystemItem": "/SAPAssetManager/Rules/Filter/FilterSystemItem.js",
                "Text": "/SAPAssetManager/Rules/Filter/FilterText.js",
                "OnPress": "/SAPAssetManager/Rules/Filter/ApplyFilterAndSave.js"
            }
        ]
    },
    "Caption":"$(L,filter)",
    "Result": "/SAPAssetManager/Rules/Equipment/EquipmentListFilterResults.js",
    "Controls":
    [{
        "Sections":
        [{
            "Controls": [{
                "_Type": "Fragment",
                "Fragment": "/SAPAssetManager/Pages/Fragments/Filter/MultiSortByFilter.fragment",
				"Properties": {
                    "Items": [
                         {
                            "ReturnValue":"EquipId",
                            "DisplayValue":"$(L,equipment_id)"
                        },
                        {
                            "ReturnValue":"EquipDesc",
                            "DisplayValue":"$(L,description)"
                        },
                        {
                            "ReturnValue":"MaintPlant",
                            "DisplayValue":"$(L,plant)"
                        },
                        {
                            "ReturnValue":"WorkCenter_Main_Nav/WorkCenterDescr",
                            "DisplayValue":"$(L,workcenter)"
                        }
                    ]
				}
            }]
        },
        {
            "Controls":
            [{
                "_Type": "Fragment",
                "Fragment": "/SAPAssetManager/Pages/Fragments/Filter/FilterByFilter.fragment",
				"Properties": {
                    "_Name": "MyEquipmentsFilter",
                    "FilterProperty":"/SAPAssetManager/Rules/Equipment/MyEquipmentsFilter.js"
				}
            },
            {
                "_Type": "Fragment",
                "Fragment": "/SAPAssetManager/Pages/Fragments/Filter/MobileStatusFilter.fragment",
				"Properties": {
                    "_Name": "StatusFilter",
                    "FilterProperty":"/SAPAssetManager/Rules/Equipment/EquipmentStatus.js"
				}
            },
            {
                "_Type": "Fragment",
                "Fragment": "/SAPAssetManager/Pages/Fragments/Filter/WorkCenterFilter.fragment",
				"Properties": {
                    "FilterProperty": "MaintWorkCenter"
				}
            }]
        }],
        "_Name":"FormCellContainer",
        "_Type":"Control.Type.FormCellContainer"
    }],
    "FioriToolbar": {
        "_Name": "EquipmentFilterFioriToolbar",
        "_Type": "Control.Type.FioriToolbar",
        "Items": [
            "/SAPAssetManager/Pages/Fragments/Filter/ResetFilterToolbarItem.fragment"
        ]
    },
    "_Type":"Page",
    "_Name":"EquipmentFilterPage",
    "OnLoaded": "/SAPAssetManager/Rules/Filter/EquipmentFilterOnLoaded.js"
}
