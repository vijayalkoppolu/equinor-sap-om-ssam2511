{
	"Caption": "$(L,work_order_transfer)",
    "OnLoaded": "/SAPAssetManager/Rules/ErrorArchive/HideCancelForErrorArchiveFix.js",
    "OnActivityBackPressed": "/SAPAssetManager/Rules/WorkOrders/CreateUpdate/CancelCheckOnBackNavAndroid.js",
    "ActionBar": {
        "_Name": "WorkOrderTransferActionBar",
        "Items":
        [{
            "Position": "Left",
            "_Name": "CancelButton",
            "SystemItem": "Cancel",
            "Caption": "$(L,cancel)",
            "OnPress": "/SAPAssetManager/Actions/Page/CancelPage.action"
        },
        {
            "Position": "Right",
            "_Name": "TransferButton",
            "Text": "$(L,transfer)",
            "OnPress": "/SAPAssetManager/Actions/WorkOrders/MobileStatus/TransferPageRequiredFields.action"
        }]
    },
    "Controls":
    [{
        "Sections":
        [{
            "Caption": "$(L, indicates_required_fields)",
            "Controls":
            [{
                "Caption": "$(L,workorder)",
                "IsEditable": false,
                "Value": "{OrderId}",
                "_Name": "WorkOrderNumber",
                "_Type": "Control.Type.FormCell.SimpleProperty"
            },
            {
                "AllowMultipleSelection": false,
                "Caption": "$(L,reason)*",
                "Value": "{TransferReason}",
                "IsEditable": true,
                "IsSelectedSectionEnabled": false,
                "IsPickerDismissedOnSelection": true,
                "IsSearchCancelledAfterSelection": true,
                "AllowDefaultValueIfOneItem": true,
                "OnValueChange": "/SAPAssetManager/Rules/Common/Validation/ResetValidationOnInput.js",
                "PickerItems": "/SAPAssetManager/Rules/MobileStatus/ReasonCodes.js",
                "Search":
                {
                    "Enabled": true
                },
                "_Name": "TransferReasonLstPkr",
                "_Type": "Control.Type.FormCell.ListPicker"
            },
            {
                "AllowMultipleSelection": false,
                "Caption": "$(L,transfer_to)*",
                "IsEditable": true,
                "Value": "/SAPAssetManager/Rules/WorkOrders/ListView/TransferToUpdate.js",
                "IsSelectedSectionEnabled": false,
                "IsPickerDismissedOnSelection": true,
                "IsSearchCancelledAfterSelection": true,
                "AllowDefaultValueIfOneItem": true,
                "OnValueChange": "/SAPAssetManager/Rules/Common/Validation/ResetValidationOnInput.js",
                "PickerItems": "/SAPAssetManager/Rules/WorkOrders/ListView/TransferToListItems.js",
                "Search":
                {
                    "Enabled": true
                },
                "_Name": "TransferToLstPkr",
                "_Type": "Control.Type.FormCell.ListPicker"
            }]
        },
        {
            "Controls":
            [{
                "IsAutoResizing": false,
                "IsEditable": true,
                "Value": "{HeaderNotes}",
                "_Name": "TransferNote",
                "Caption": "$(L,notes)",
                "PlaceHolder": "$(L,notes)",
                "_Type": "Control.Type.FormCell.Note"
            }]
        },
        {
            "Controls":[
                "/SAPAssetManager/Pages/Fragments/DiscardButton.fragment"
            ]
        }],
        "_Name": "FormCellContainer",
        "_Type": "Control.Type.FormCellContainer"
    }],
	"_Name": "WorkOrderTransfer",
	"_Type": "Page"
}