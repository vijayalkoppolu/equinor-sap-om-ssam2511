                                    "Style": "GridTableHeaderText"
								},
								{
									"Text": "$(L,workorder)",
									"TextAlignment": "Center",
                                    "Style": "GridTableHeaderText"
								},
								{
									"Text": "$(L,operation)",
									"TextAlignment": "Center",
                                    "Style": "GridTableHeaderText"
								},
								{
									"Text": "$(L,hours)",
									"TextAlignment": "Center",
                                    "Style": "GridTableHeaderText"
								}
							]
						}
					},
					"Row": {
						"Layout": {
                           "ColumnWidth": [200,200,200,200,200,-1]
