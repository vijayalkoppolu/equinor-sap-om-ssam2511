{
	"Caption": "$(L,inbound_delivery_items_title)",
	"ActionBar": {
		"Items": [
			"/SAPAssetManager/Pages/Fragments/SyncButton.fragment"
		]
	},
	"Controls": [
		{
			"FilterFeedbackBar": {
				"_Type": "Control.Type.FilterFeedbackBar",
				"_Name": "FilterFeedback",
				"ShowAllFilters": true,
				"FastFilters": [
					{
						"_Name": "UnpackedIBD",
						"_Type": "Control.Type.FastFilterItem",
						"FilterType": "Filter",
						"CustomQueryGroup": "WHIBD",
						"DisplayValue": "/SAPAssetManager/Rules/EWM/Inbound/Items/WHGetInboundDeliveryItemUnpackedCaption.js",
						"ReturnValue": "/SAPAssetManager/Rules/EWM/Inbound/Items/WHInboundDeliveryItemFilterUnpacked.js"
					},
					{
						"_Name": "PackedIBD",
						"_Type": "Control.Type.FastFilterItem",
						"FilterType": "Filter",
						"CustomQueryGroup": "WHIBD",
						"DisplayValue": "/SAPAssetManager/Rules/EWM/Inbound/Items/WHGetInboundDeliveryItemPackedCaption.js",
						"ReturnValue": "/SAPAssetManager/Rules/EWM/Inbound/Items/WHInboundDeliveryItemFilterPacked.js"
					},
					{
						"_Name": "NotPackableIBD",
						"_Type": "Control.Type.FastFilterItem",
						"FilterType": "Filter",
						"CustomQueryGroup": "WHIBD",
						"DisplayValue": "/SAPAssetManager/Rules/EWM/Inbound/Items/WHInboundDeliveryItemNotPackableCaption.js",
						"ReturnValue": "/SAPAssetManager/Rules/EWM/Inbound/Items/WHInboundDeliveryItemFilterNotPackable.js"
					}
				]
			},
			"Sections": [
				{
					"EmptySection": {
						"Caption": "$(L,no_inbound_delivery_item)"
					},
					"Header": {
						"Caption": "/SAPAssetManager/Rules/EWM/Inbound/Items/WHInboundDeliveryItemCaptionCount.js",
						"UseTopPadding": false,
						"Items": [
							{
								"_Type": "SectionHeaderItem.Type.Label",
								"_Name": "CaptionLabel",
								"Title": "/SAPAssetManager/Rules/EWM/Inbound/Items/WHInboundDeliveryItemCaptionCount.js",
								"Position": "Left"
							},
							{
								"_Type": "SectionHeaderItem.Type.Button",
								"_Name": "SelectItems",
								"Title": "$(L, select_items)",
								"Position": "Right",
								"Visible": "/SAPAssetManager/Rules/EWM/InboundDelivery/InboundDeliveryDetailsView.js",
								"OnPress": "/SAPAssetManager/Rules/EWM/Inbound/Items/WHInboundDeliveryItemListHeaderButtonOnPress.js",
								"Styles": {
                                    "Button": "SelectItemsButtonLabelFontSize"
                                }
							},
							{
								"_Type": "SectionHeaderItem.Type.Button",
								"_Name": "SelectAll",
								"Title": "$(L, select_all)",
								"Position": "Right",
								"Visible": false,
								"OnPress": "/SAPAssetManager/Rules/EWM/Inbound/Items/WHInboundDeliveryItemListHeaderButtonOnPress.js",
								"Styles": {
                                    "Button": "SelectItemsButtonLabelFontSize"
                                }
							},
							{
								"_Type": "SectionHeaderItem.Type.Button",
								"_Name": "DeselectAll",
								"Title": "$(L, deselect_all)",
								"Position": "Right",
								"Visible": false,
								"OnPress": "/SAPAssetManager/Rules/EWM/Inbound/Items/WHInboundDeliveryItemListHeaderButtonOnPress.js",
								"Styles": {
                                    "Button": "SelectItemsButtonLabelFontSize"
                                }
							}
                        ]
					},
					"Selection": {	
						"LongPressToEnable": "/SAPAssetManager/Rules/EWM/Inbound/Items/WHInboundDeliveryItemListSelectionLongPressToEnable.js",
						"ExitOnLastDeselect": true			
					},
					"OnSelectionModeChanged": "/SAPAssetManager/Rules/EWM/Inbound/Items/WHInboundDeliveryItemListOnSelectionModeChanged.js",
					"OnSelectionChanged": "/SAPAssetManager/Rules/EWM/Inbound/Items/WHInboundDeliveryItemListOnSelectionChanged.js",
					"Search": {
						"BarcodeScanner": true,
						"Enabled": true,
						"MinimumCharacterThreshold": 2,
						"Placeholder": "$(L,search)",
						"Delay": 500
					},
					"ObjectCell":"/SAPAssetManager/Pages/Fragments/EWM/InboundDeliveryItemCell.fragment",
					"Target": {
						"EntitySet": "WarehouseInboundDeliveryItems",
						"QueryOptions": "/SAPAssetManager/Rules/EWM/Inbound/Items/WHInboundDeliveryItemListQuery.js",
						"Service": "/SAPAssetManager/Services/AssetManager.service"
					},
					"_Name": "SectionObjectTable",
					"_Type": "Section.Type.ObjectTable"
				}
			],
			"_Name": "SectionedTable",
			"_Type": "Control.Type.SectionedTable",
			"Filters": "/SAPAssetManager/Rules/EWM/Inbound/Items/GetWHIBDItemPreselectedFilter.js"
		}
	],
	"FioriToolbar": {
		"_Name": "ItemsFioriToolbar",
		"_Type": "Control.Type.FioriToolbar",
		"Visible": "/SAPAssetManager/Rules/EWM/InboundDelivery/InboundDeliveryDetailsView.js",
		"DataSubscriptions": ["WarehouseInboundDeliveries"],
		"Items": [
			{
				"_Type": "FioriToolbarItem.Type.Button",
				"_Name": "GoodsReceiptButton",
				"ButtonType": "Primary",
				"Title": "$(L, goods_receipt)",
				"OnPress": "/SAPAssetManager/Rules/EWM/Inbound/WHInboundDeliveryGoodsReceiptOnPress.js"
			},
			{
				"_Type": "FioriToolbarItem.Type.Button",
				"_Name": "AutoPackButton",
				"ButtonType": "Secondary",
				"Title": "$(L, auto_pack)",
				"OnPress": "/SAPAssetManager/Rules/EWM/InboundDelivery/InboundDeliveryAutoPackOnPress.js"
			},
			{
				"_Type": "FioriToolbarItem.Type.Button",
				"_Name": "CreateMixedHUButton",
				"ButtonType": "Primary",
				"Semantic": "Normal",
				"Title": "$(L, create_mixed_handling_unit)",
				"Visible": false,
				"Enabled": false,
				"OnPress": "/SAPAssetManager/Rules/EWM/Inbound/Items/HandlingUnit/WHHandlingUnitMixedCreateNav.js"
			},
			{
		        "_Name": "AutoCreateTaskButton",
		        "_Type": "FioriToolbarItem.Type.Button",
		        "Title": "$(L, auto_task_create)",
		        "ButtonType": "Secondary",
		        "OnPress": "/SAPAssetManager/Rules/EWM/WarehouseTask/WarehouseTaskOnAutoCreatePress.js",
		        "Visible": true,
				"Enabled": "/SAPAssetManager/Rules/EWM/WarehouseTask/AutoCreateWarehouseTaskIsEnabled.js"
		    },
			{
		        "_Name": "EditAllItemsButton",
		        "_Type": "FioriToolbarItem.Type.Button",
		        "Title": "$(L, ewm_edit_all_items)",
		        "ButtonType": "Text",
		        "OnPress": "/SAPAssetManager/Rules/EWM/Inbound/Items/WHInboundDeliveryEditAllItemsOnPress.js",
		       	"Visible": true
		    }
		]
	},
	"OnLoaded": "/SAPAssetManager/Rules/Filter/ListPageWithFilterOnLoaded.js",
	"DataSubscriptions": ["HandlingUnits", "WarehouseTasks", "HandlingUnitItems", "WarehouseInboundDeliveryItems", "WarehouseInboundDeliveries"],	
	"_Name": "EWMInboundDeliveryItemListPage",
	"_Type": "Page"
}