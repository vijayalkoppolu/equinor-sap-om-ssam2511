                            "_Name": "DestinationHU",
                            "_Type": "Control.Type.FormCell.ListPicker",
                            "Caption": "$(L,destination_hu)",
                            "AllowMultipleSelection": true,
                            "IsPickerDismissedOnSelection": true,
                            "IsSearchCancelledAfterSelection": true,
                            "Search": {
                                "Enabled": true,
                                "Delay": 500,
                                "MinimumCharacterThreshold": 3,
                                "Placeholder": "$(L,search)",
                                "BarcodeScanner": false
                            },
                            "PickerItems": "/SAPAssetManager/Rules/EWM/WarehouseTask/SearchFilter/DestinationHUPickerItems.js",
                            "FilterProperty": "DestinationHU",
                            "OnValueChange": "/SAPAssetManager/Rules/Filter/RedrawFilterToolbar.js"
                        },
                        {
                            "_Name": "SourceBin",
                            "_Type": "Control.Type.FormCell.ListPicker",
                            "Caption": "$(L,source_bin)",
                            "AllowMultipleSelection": true,
                            "IsPickerDismissedOnSelection": true,
                            "IsSearchCancelledAfterSelection": true,
                            "Search": {
                                "Enabled": true,
                                "Delay": 500,
                                "MinimumCharacterThreshold": 3,
                                "Placeholder": "$(L,search)",
                                "BarcodeScanner": false
                            },
                            "PickerItems": "/SAPAssetManager/Rules/EWM/WarehouseTask/SearchFilter/SourceBinPickerItems.js",
                            "FilterProperty": "SourceBin",
                            "OnValueChange": "/SAPAssetManager/Rules/Filter/RedrawFilterToolbar.js"
                        },
                        {
                            "_Name": "DestinationBin",
                            "_Type": "Control.Type.FormCell.ListPicker",
                            "Caption": "$(L,destination_bin)",
                            "AllowMultipleSelection": true,
                            "IsPickerDismissedOnSelection": true,
                            "IsSearchCancelledAfterSelection": true,
                            "Search": {
                                "Enabled": true,
                                "Delay": 500,
                                "MinimumCharacterThreshold": 3,
                                "Placeholder": "$(L,search)",
                                "BarcodeScanner": false
                            },
                            "PickerItems": "/SAPAssetManager/Rules/EWM/WarehouseTask/SearchFilter/DestinationBinPickerItems.js",
                            "FilterProperty": "DestinationBin",
                            "OnValueChange": "/SAPAssetManager/Rules/Filter/RedrawFilterToolbar.js"
                        }

                    ]
                }
            ],
            "_Name": "FormCellContainer",
            "_Type": "Control.Type.FormCellContainer"
        }
    ],
    "FioriToolbar": {
        "_Name": "StockSearchFilterFioriToolbar",
        "_Type": "Control.Type.FioriToolbar",
        "Items": [
            {
                "_Type": "Fragment",
                "Fragment": "/SAPAssetManager/Pages/Fragments/Filter/ResetFilterToolbarItem.fragment"
            }
        ]
    },
    "_Type": "Page",
    "_Name": "WarehouseTaskSearchFilterPage",
    "OnLoaded": "/SAPAssetManager/Rules/EWM/WarehouseTask/SearchFilter/WarehouseTaskSearchFilterOnLoaded.js"
}