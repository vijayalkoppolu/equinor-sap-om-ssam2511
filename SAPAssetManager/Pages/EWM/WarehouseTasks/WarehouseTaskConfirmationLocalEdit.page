{
	"Caption": "/SAPAssetManager/Rules/EWM/WarehouseTask/Confirmation/SetCaptionForUpdate.js",
    "OnActivityBackPressed": "/SAPAssetManager/Rules/WorkOrders/CreateUpdate/CancelCheckOnBackNavAndroid.js",
    "ActionBar": {
        "Items": [
            {
                "Position": "Left",
                "SystemItem": "Cancel",
                "Caption": "$(L,cancel)",
                "OnPress": "/SAPAssetManager/Rules/EWM/WarehouseTask/Confirmation/OnPressWarehouseTaskConfirmationCancelButton.js"
            },
            {
                "Position": "Right",
                "SystemItem": "$(PLT,'Confirm','', 'Confirm')",
                "Caption": "$(L,confirm)",
                "Text": "$(L,confirm)",
				"OnPress": "/SAPAssetManager/Rules/EWM/WarehouseTask/Confirmation/OnPressWarehouseTaskConfirmationDoneButton.js"
            }
        ]
	},
	"Controls": [
		{
			"Sections": [
				{
					"Header": {
                        "UseTopPadding": false
                    },
					"Controls": [
						{
							"Caption": "$(L,warehouse_task_num)/$(L,confirmation_item)",
							"IsEditable": false,
							"KeyboardType": "Default",
							"Value": "$(DV, '/SAPAssetManager/Rules/EWM/WarehouseTask/WarehouseTaskNumberWithoutLeadingZeros.js', '-')/$(DV, {WarehouseTaskItem}, '-')",
							"_Name": "WhTaskNumSimple",
							"_Type": "Control.Type.FormCell.SimpleProperty"
						},
						{
							"Caption": "$(L,process_category)/$(L,activity_area)/$(L,hdr_warehouse_proctype)",
							"IsEditable": false,
							"KeyboardType": "Default",
							"Value": "$(DV,{WarehouseTask_Nav/WarehouseProcessCategory_Nav/Description}, '-')/$(DV, {WarehouseTask_Nav/ActivityArea},'-')/$(DV, {WarehouseTask_Nav/WarehouseProcessType_Nav/WarehouseProcessTypeDescription},'-')",
							"_Name": "WhProcCategoryActivityProcessingTypeSimple",
							"_Type": "Control.Type.FormCell.SimpleProperty"
						},
						{
							"Caption": "$(L,product)/$(L,product_description)",
							"IsEditable": false,
							"KeyboardType": "Default",
							"Value": "$(DV, {WarehouseTask_Nav/Product}, '-')/$(DV, {WarehouseTask_Nav/ProductDescription}, '-')",
							"_Name": "WhProductDescriptionSimple",
							"_Type": "Control.Type.FormCell.SimpleProperty"
						},
						{
							"Caption": "$(L,batch)",
							"IsEditable": false,
							"IsVisible": "/SAPAssetManager/Rules/EWM/WarehouseTask/Confirmation/IsVisibleBatch.js",
							"KeyboardType": "Default",
							"Value": "{Batch}",
							"_Name": "WhBatchSimple",
							"_Type": "Control.Type.FormCell.SimpleProperty"
						},
						{
                            "_Name": "ExceptionPicker",
                            "_Type": "Control.Type.FormCell.ListPicker",
                            "Caption": "$(L,exception_handling)",
                            "AllowEmptySelection": true,
                            "AllowMultipleSelection": false,
                            "IsPickerDismissedOnSelection": true,
                            "AllowDefaultValueIfOneItem": true,
                            "IsSearchCancelledAfterSelection": true,
                            "Search": {
                                "Enabled": true,
                                "Delay": 500,
                                "MinimumCharacterThreshold": 2,
                                "Placeholder": "$(L,search)"
                            },
                            "IsEditable": true,
                            "IsVisible": true,
                            "PickerItems": {
                                "DisplayValue": "{{#Property:ExceptionCode}} - {{#Property:Description}}",
                                "ReturnValue": "{ExceptionCode}",
                                "Target": {
                                    "EntitySet": "WarehouseExceptionCodes",
                                    "Service": "/SAPAssetManager/Services/AssetManager.service",
									"QueryOptions": "$filter=WarehouseNo eq '{WarehouseNo}' and ProcCategory eq '{WarehouseTask_Nav/ProcCategory}'"
                                    }
                            },
                            "PickerPrompt": "$(L,select_single_item)",
							"Value": "$(DV, {MultiExceptionCodes}, '-')",
							"OnValueChange": "/SAPAssetManager/Rules/EWM/WarehouseTask/Confirmation/OnValueChangeExceptionCode.js"
                        },
						{
							"Caption": "$(L, to_confirm_quantity)",
							"IsEditable": "/SAPAssetManager/Rules/EWM/WarehouseTask/Confirmation/QuantityIsEditable.js",
							"HelperText": "/SAPAssetManager/Rules/EWM/WarehouseTask/Confirmation/GetQuantityHelperText.js",
							"KeyboardType": "Number",
							"Value": "/SAPAssetManager/Rules/EWM/WarehouseTask/Confirmation/GetQuantityValue.js",
							"_Name": "WhQuantitySimple",
							"_Type": "Control.Type.FormCell.SimpleProperty"
						},
						{
							"Caption": "$(L,uom)",
							"IsEditable": false,
							"KeyboardType": "Default",
							"Value": "{WarehouseTask_Nav/UoM}",
							"_Name": "WhUoMSimple",
							"_Type": "Control.Type.FormCell.SimpleProperty"
						},
						{
                            "_Type": "Control.Type.FormCell.Button",
                            "_Name": "SerialNumberAdd",
                            "Title": "$(L,serial_add_serial_number)",
                            "OnPress": "/SAPAssetManager/Actions/EWM/WarehouseTasks/SerialNumber/SerialNumberNav.action",
                            "TextAlignment": "Center",
							"ButtonType": "Text",
							"IsVisible": "/SAPAssetManager/Rules/EWM/WarehouseTask/Confirmation/IsVisibleSerialNumberButtonLocalConfirmation.js"
                        },
						{
                            "_Type": "Control.Type.FormCell.Button",
                            "_Name": "SerialNumberDiffAdd",
                            "Title": "$(L,ewm_add_serial_number_diff)",
                            "OnPress": "/SAPAssetManager/Actions/EWM/WarehouseTasks/SerialNumber/SerialNumberNav.action",
                            "TextAlignment": "Center",
							"ButtonType": "Text",
							"IsVisible": false
                        },
						{
							"Caption": "$(L,source_bin)",
							"IsEditable": false,
							"KeyboardType": "Default",
							"Value": "{WarehouseTask_Nav/SourceBin}",
							"_Name": "WhSourceBinSimple",
							"_Type": "Control.Type.FormCell.SimpleProperty"
						},
						{
							"Caption": "$(L,source_hu)",
							"IsEditable": false,
							"KeyboardType": "Default",
							"AlternateInput": "Barcode",
							"Value": "{SrcHU}",
							"_Name": "WhSourceHUSimple",
							"_Type": "Control.Type.FormCell.SimpleProperty"
						},
						{
							"Caption": "$(L,remove_hu)",
							"IsEditable": true,
							"KeyboardType": "Default",
							"Value": "$(DV, {WithdrawHU}, 'false')",
							"IsVisible": "/SAPAssetManager/Rules/EWM/WarehouseTask/Confirmation/IsVisibleRemoveHuSwitch.js",
							"_Name": "WhRemoveHuSwitch",
							"_Type": "Control.Type.FormCell.Switch"
						},
						{
							"Caption": "$(L,destination_bin)",
							"IsEditable": true,
							"KeyboardType": "Default",
							"AlternateInput": "Barcode",
							"Value": "{DestinationBin}",
							"HelperText": "/SAPAssetManager/Rules/EWM/WarehouseTask/Confirmation/GetDestinationBinHelperText.js",
							"_Name": "WhDestinationBinSimple",
							"_Type": "Control.Type.FormCell.SimpleProperty"
						},
						"/SAPAssetManager/Pages/Fragments/EWM/DestinationBinPickerButton.fragment",
						{
                            "_Name": "WhDestinationHUPicker",
                            "_Type": "Control.Type.FormCell.ListPicker",
                            "Caption": "$(L,destination_hu)",
                            "AllowEmptySelection": true,
                            "AllowMultipleSelection": false,
                            "IsPickerDismissedOnSelection": true,
                            "AllowDefaultValueIfOneItem": true,
                            "IsSearchCancelledAfterSelection": true,
                            "Search": {
                                "Enabled": true,
                                "Delay": 500,
                                "MinimumCharacterThreshold": 2,
                                "Placeholder": "$(L,search)",
                                "BarcodeScanner": true
                            },
                            "IsEditable": true,
                            "IsVisible": "/SAPAssetManager/Rules/EWM/WarehouseTask/Confirmation/IsVisibleHandlingUnitButton.js",
							"PickerItems": "/SAPAssetManager/Rules/EWM/WarehouseTask/Confirmation/GetPickerItemsForHU.js",
                    		"PickerPrompt": "$(L,select_single_item)",
							"Value": "{DestHU}"
                		},
						{
                            "_Type": "Control.Type.FormCell.Button",
                            "_Name": "MaintainPickHandlingUnit",
                            "Title": "$(L,maintain_handling_unit)",
                            "OnPress": "/SAPAssetManager/Actions/EWM/WarehouseTasks/HandlingUnit/HandlingUnitNav.action",
                            "TextAlignment": "Center",
							"ButtonType": "Text",
							"IsVisible": "/SAPAssetManager/Rules/EWM/WarehouseTask/Confirmation/IsVisibleHandlingUnitButton.js"
                        },
                        {
							"Caption": "$(L, warehouse_order)",
							"IsEditable": false,
							"KeyboardType": "Default",
							"Value": "{WarehouseTask_Nav/WarehouseOrder}",
							"_Name": "WhOrderSimple",
							"_Type": "Control.Type.FormCell.SimpleProperty"
						},
                        {
							"Caption": "$(L, delivery)",
							"IsEditable": false,
							"KeyboardType": "Default",
							"Value": "{WarehouseTask_Nav/Delivery}",
							"_Name": "WhDeliverySimple",
							"_Type": "Control.Type.FormCell.SimpleProperty"
						},
                        {
							"Caption": "$(L, weight)",
							"IsEditable": false,
							"KeyboardType": "Default",
							"Value": "{WarehouseTask_Nav/LoadingWeight} {WarehouseTask_Nav/WeightUnit}",
							"_Name": "WhLoadingWeightSimple",
							"_Type": "Control.Type.FormCell.SimpleProperty"
						},
                        {
							"Caption": "$(L, volume)",
							"IsEditable": false,
							"KeyboardType": "Default",
							"Value": "{WarehouseTask_Nav/LoadingVolume} {WarehouseTask_Nav/VolumeUnit}",
							"_Name": "WhLoadingVolumeSimple",
							"_Type": "Control.Type.FormCell.SimpleProperty"
						}
					]
				}
			],
            "_Name": "FormCellContainer",
            "_Type": "Control.Type.FormCellContainer"
		}
	],
	"ToolBar": {
		"Controls": [
			{
				"_Name": "FlexibleSpaceCenterTbI",
				"_Type": "Control.Type.ToolbarItem",
				"SystemItem": "FlexibleSpace",
				"Visible": "/SAPAssetManager/Rules/Common/IsFlexibleSpaceVisible.js"
			},			
			{
				"_Name": "Discard",
				"_Type": "Control.Type.ToolbarItem",
				"Caption": "$(L, discard)",
				"OnPress": "/SAPAssetManager/Rules/EWM/WarehouseTask/Confirmation/OnPressWarehouseTaskConfirmationDiscardButton.js"
			}
		]
	},
	"_Name": "WarehouseTaskConfirmationLocalEditPage",
	"DataSubscriptions": ["WarehouseTasks", "WarehousePickHUs"],
	"OnLoaded": "/SAPAssetManager/Rules/EWM/WarehouseTask/Confirmation/OnPageLoadWarehouseTaskConfirmation.js",
	"_Type": "Page"
}