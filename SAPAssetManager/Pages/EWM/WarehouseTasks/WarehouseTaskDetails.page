{
	"Caption": "$(L, warehouse_task)",
	"ActionBar": {
		"Items": [
			{
				"Position": "Right",
				"Icon": "$(PLT, sap-icon://edit, sap-icon://edit, '', sap-icon://edit)",
				"Caption": "$(L, ewm_confirm_task)",
				"Visible": true,
				"OnPress": "/SAPAssetManager/Rules/EWM/WarehouseTask/Confirmation/OnPressTaskConfirmationButton.js"
			},
			"/SAPAssetManager/Pages/Fragments/SyncButton.fragment"
		]
	},
	"Controls": [
		{
			"Sections": [
				{
					"_Type": "Section.Type.KeyValue",
					"_Name": "WarehouseTaskDetails0",
					"Header": {
						"Caption": "$(L,item_details)"
					},
					"KeyAndValues": [
						{
							"_Name": "WarehouseTask",
							"KeyName": "$(L,warehouse_task_num)",
							"Value": "$(DV, '/SAPAssetManager/Rules/EWM/WarehouseTask/WarehouseTaskNumberWithoutLeadingZeros.js', '-')"
						},
						{
							"_Name": "WarehouseOrder",
							"KeyName": "$(L,warehouse_ord_num)",
							"Value": "$(DV, {WarehouseOrder}, '-')"
						},
						{
							"_Name": "LEDelivery",
							"KeyName": "$(L,le_delivery)",
							"Value": "$(DV, {Delivery}, '-')"
						},
						{
							"_Name": "EWMDelivery",
							"KeyName": "$(L,ewm_delivery)",
							"Value": "$(DV, '/SAPAssetManager/Rules/EWM/WarehouseTask/GetEWMDelivery.js', '-')",

							"OnPress": "/SAPAssetManager/Rules/EWM/WarehouseTask/Details/NavigateToEWMDelivery.js"
						},
					    {
							"_Name": "PurchaseOrder",
							"KeyName": "$(L,purchase_order)",
							"Value": "/SAPAssetManager/Rules/EWM/WarehouseTask/GetPO.js"
						},
						{
							"_Name": "MaintenanceOrder",
							"KeyName": "$(L,maintenance_order)",
							"Value": "$(DV,{MaintenanceOrder}, '-')"
						},
						{
							"_Name": "WarehouseNum",
							"KeyName": "$(L,warehouse_num)",
							"Value": "$(DV, '/SAPAssetManager/Rules/EWM/Common/WarehouseNumberDescription.js', '-')"
						},
						{
							"_Name": "ActivityArea",
							"KeyName": "$(L,activity_area)",
							"Value": "$(DV,{ActivityArea}, '-')"
						},
						{
							"_Name": "ProcCategory",
							"KeyName": "$(L,process_category)",
							"Value": "$(DV,{WarehouseProcessCategory_Nav/Description}, '-')"
						},
						{
							"_Name": "ProductDescription",
							"KeyName": "$(L,product_description)",
							"Value": "$(DV,'/SAPAssetManager/Rules/EWM/WarehouseTask/ListView/WarehouseTaskTitle.js', '-')"
						},
						{
							"_Name": "Batch",
							"KeyName": "$(L,batch)",
							"Value": "$(DV,{Batch},'-')"
						},
						{
							"_Name": "Quantity",
							"KeyName": "$(L,quantity)",
							"Value": "$(DV,{Quantity},'-')"
						},
						{
							"_Name": "UoM",
							"KeyName": "$(L,uom)",
							"Value": "$(DV,{UoM},'-')"
						},
						{
							"_Name": "LoadingWeight",
							"KeyName": "$(L, weight)",
							"Value": "$(DV,'/SAPAssetManager/Rules/EWM/WarehouseTask/Details/WeightValue.js', '-')"
						},
						{
							"_Name": "LoadingVolume",
							"KeyName": "$(L, volume)",
							"Value": "$(DV,'/SAPAssetManager/Rules/EWM/WarehouseTask/Details/VolumeValue.js', '-')"
						},
						{
							"_Name": "SrceStorType",
							"KeyName": "$(L,src_stor_type_section)",
							"Value": "$(DV,{SrceStorType}, '-')/$(DV, {SrcStorSectn},'-')"
						},
						{
							"_Name": "SourceBin",
							"KeyName": "$(L,source_bin)",
							"Value": "$(DV,{SourceBin},'-')"
						},
						{
							"_Name": "SourceHU",
							"KeyName": "$(L,source_hu)",
							"Value": "$(DV,{SourceHU},'-')"
						},
						{
							"_Name": "DestStoreType",
							"KeyName": "$(L,dest_stor_type_section)",
							"Value": "$(DV, {DestStoreType},'-')/$(DV, {DestSection},'-')"
						},
						{
							"_Name": "DestinationBin",
							"KeyName": "$(L,destination_bin)",
							"Value": "$(DV,{DestinationBin},'-')"
						},
						{
							"_Name": "DestinationHU",
							"KeyName": "$(L,destination_hu)",
							"Value": "$(DV, {DestinationHU},'-')"
						},
						{
							"_Name": "Resource",
							"KeyName": "$(L,resource)",
							"Value": "$(DV, {Resource},'-')"
						},
						{
							"_Name": "ExceptionCode",
							"KeyName": "$(L,exception_handling)",
							"Value": "$(DV, {ExceptionCode},'-')"
						},
						{
							"_Name": "MovementReason",
							"KeyName": "$(L, movement_reason)",
							"Value": "$(DV, {MovementReason},'-')"
						}
					],
					"Target": {
                        "EntitySet": "{@odata.readLink}",
                        "Service": "/SAPAssetManager/Services/AssetManager.service",
                        "QueryOptions": "$expand=WarehouseTaskConfirmation_Nav,WarehouseTaskSerialNumber_Nav,WarehouseProcessCategory_Nav,WarehouseProcessType_Nav"
					},
					"Visible": true,
					"DataSubscriptions": [
						"WarehouseTasks"
					]
				},
				"/SAPAssetManager/Pages/Fragments/EWM/SerialNumberDisplay.fragment"
			],
			"_Type": "Control.Type.SectionedTable",
			"_Name": "SectionedTable"
		}
	],
	"ToolBar": {
		"Controls": [
			{
				"_Name": "PreviousTask",
				"_Type": "Control.Type.ToolbarItem",
				"Caption": "$(L, previous_item)",
				"OnPress": "/SAPAssetManager/Rules/EWM/WarehouseTask/Details/OnPressPreviousNextButton.js",
				"Enabled": "/SAPAssetManager/Rules/EWM/WarehouseTask/Details/IsEnabledPreviousButton.js",
				"Visible": "/SAPAssetManager/Rules/EWM/WarehouseTask/Details/IsVisiblePreviousNextButton.js"
			},
			"/SAPAssetManager/Pages/Fragments/Inventory/FlexibleSpaceToolBar.fragment",
			{
				"_Name": "NextTask",
				"_Type": "Control.Type.ToolbarItem",
				"Caption": "$(L, next_item)",
				"OnPress": "/SAPAssetManager/Rules/EWM/WarehouseTask/Details/OnPressPreviousNextButton.js",
				"Enabled": "/SAPAssetManager/Rules/EWM/WarehouseTask/Details/IsEnabledNextButton.js",
				"Visible": "/SAPAssetManager/Rules/EWM/WarehouseTask/Details/IsVisiblePreviousNextButton.js"
			}
		]
	},
	"_Name": "WarehouseTaskDetails",
	"DataSubscriptions": [
		"WarehouseTasks",
		"WarehouseTaskConfirmations",
		"WarehouseTaskSerialNumber",
		"WarehouseExceptionCodes",
		"WarehouseStorageBins",
		"WarehouseMovementReasons",
		"WarehouseActivityAreas"
	],
	"_Type": "Page"
}