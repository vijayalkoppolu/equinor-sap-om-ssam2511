{
    "OnActivityBackPressed": "/SAPAssetManager/Rules/WorkOrders/CreateUpdate/CancelCheckOnBackNavAndroid.js",
    "ActionBar": {
        "Items": [
            {
                "Position": "Left",
                "SystemItem": "Cancel",
                "Caption": "$(L,cancel)",
                "OnPress": "/SAPAssetManager/Actions/Page/CancelPage.action"
            },
            {
                "Position": "Right",
                "SystemItem": "/SAPAssetManager/Rules/Filter/FilterSystemItem.js",
                "Text": "/SAPAssetManager/Rules/Filter/FilterText.js",
                "OnPress": "/SAPAssetManager/Rules/Filter/ApplyFilterAndSave.js"
            }
        ]
    },
    "Caption": "$(L,filter)",
    "Result": "/SAPAssetManager/Rules/EWM/WarehouseOrder/SearchFilter/WarehouseOrderSearchFilterResults.js",
    "Controls": [
        {
            "Sections": [
                {
                    "Controls": [
                        {
                            "_Type": "Fragment",
                            "Fragment": "/SAPAssetManager/Pages/Fragments/Filter/MultiSortByFilter.fragment",
                            "Properties": {
                                "Items": [
                                    {
                                        "ReturnValue": "WarehouseOrder",
                                        "DisplayValue": "$(L,warehouse_order)"
                                    },
                                    {
                                        "ReturnValue": "WarehouseTask_Nav/DestinationBin",
                                        "DisplayValue": "$(L,destination_bin)"
                                    },
                                    {
                                        "ReturnValue": "WOProcessType",
                                        "DisplayValue": "$(L,hdr_warehouse_proctype)"
                                    }
                                ]
                            }
                        }
                    ]
                },
                {
                    "Caption": "$(L,filter_by)",
                    "Controls": [
                        {
                            "_Name": "WarehouseProcessCategory",
                            "_Type": "Control.Type.FormCell.ListPicker",
                            "Caption": "$(L,process_category)",
                            "AllowMultipleSelection": true,
                            "IsPickerDismissedOnSelection": true,
                            "IsSearchCancelledAfterSelection": true,
                            "Search": {
                                "Enabled": true,
                                "Delay": 500,
                                "MinimumCharacterThreshold": 3,
                                "Placeholder": "$(L,search)",
                                "BarcodeScanner": false
                            },
                            "PickerItems": "/SAPAssetManager/Rules/EWM/WarehouseOrder/SearchFilter/ProcessCategoryPickerItems.js",
                            "FilterProperty": "ProcCategory",
                            "OnValueChange": "/SAPAssetManager/Rules/Filter/RedrawFilterToolbar.js"
                        },
                        {
                            "_Name": "WarehouseOrderNumber",
                            "_Type": "Control.Type.FormCell.ListPicker",
                            "Caption": "$(L,warehouse_ord_num)",
                            "AllowMultipleSelection": true,
                            "IsPickerDismissedOnSelection": true,
                            "IsSearchCancelledAfterSelection": true,
                            "Search": {
                                "Enabled": true,
                                "Delay": 500,
                                "MinimumCharacterThreshold": 3,
                                "Placeholder": "$(L,search)",
                                "BarcodeScanner": false
                            },
                            "PickerItems": "/SAPAssetManager/Rules/EWM/WarehouseOrder/SearchFilter/WarehouseOrderNumberPickerItems.js",
                            "FilterProperty": "WarehouseOrder",
                            "OnValueChange": "/SAPAssetManager/Rules/Filter/RedrawFilterToolbar.js"
                        },
                        {
                            "_Name": "PurchaseOrderNumber",
                            "_Type": "Control.Type.FormCell.ListPicker",
                            "IsVisible": false,
                            "IsEditable": false,
                            "Caption": "$(L,ewm_purchase_order_number)",
                            "AllowMultipleSelection": true,
                            "IsPickerDismissedOnSelection": true,
                            "IsSearchCancelledAfterSelection": true,
                            "Search": {
                                "Enabled": true,
                                "Delay": 500,
                                "MinimumCharacterThreshold": 3,
                                "Placeholder": "$(L,search)",
                                "BarcodeScanner": false
                            },
                            "PickerItems": {
                                "DisplayValue": "{{#Property:ReferenceDoc}}",
                                "ReturnValue": "{ReferenceDoc}",
                                "Target": {
                                    "EntitySet": "WarehouseOrders",
                                    "Service": "/SAPAssetManager/Services/AssetManager.service",
                                    "QueryOptions": "$orderby=WarehouseOrder"
                                }
                            },
                            "FilterProperty": "WarehouseOrder",
                            "OnValueChange": "/SAPAssetManager/Rules/Filter/RedrawFilterToolbar.js"
                        },
                        {
                            "_Name": "Product",
                            "_Type": "Control.Type.FormCell.ListPicker",
                            "Caption": "$(L,product)",
                            "AllowMultipleSelection": true,
                            "IsPickerDismissedOnSelection": true,
                            "IsSearchCancelledAfterSelection": true,
                            "Search": {
                                "Enabled": true,
                                "Delay": 500,
                                "MinimumCharacterThreshold": 3,
                                "Placeholder": "$(L,search)",
                                "BarcodeScanner": false
                            },
                            "PickerItems": "/SAPAssetManager/Rules/EWM/WarehouseOrder/SearchFilter/ProductPickerItems.js",
                            "FilterProperty": "Product",
                            "OnValueChange": "/SAPAssetManager/Rules/Filter/RedrawFilterToolbar.js"
                        },
                        {
                            "Caption": "$(L,ewm_le_delivery_number)",
							"IsEditable": true,
							"KeyboardType": "Default",
							"AlternateInput": "Barcode",
							"Value": "/SAPAssetManager/Rules/EWM/WarehouseOrder/SearchFilter/LEDeliveryNumberValue.js",
							"_Name": "LEDeliveryNumber",
							"_Type": "Control.Type.FormCell.SimpleProperty",
                            "FilterProperty": "ReferenceDoc",
                            "OnValueChange": "/SAPAssetManager/Rules/Filter/RedrawFilterToolbar.js"
                        },
                        {
                            "_Name": "Queue",
                            "_Type": "Control.Type.FormCell.ListPicker",
                            "Caption": "$(L,queue)",
                            "AllowMultipleSelection": true,
                            "IsPickerDismissedOnSelection": true,
                            "IsSearchCancelledAfterSelection": true,
                            "Search": {
                                "Enabled": true,
                                "Delay": 500,
                                "MinimumCharacterThreshold": 3,
                                "Placeholder": "$(L,search)",
                                "BarcodeScanner": false
                            },
                            "PickerItems": "/SAPAssetManager/Rules/EWM/WarehouseOrder/SearchFilter/QueuePrickerItems.js",
                            "FilterProperty": "Queue",
                            "OnValueChange": "/SAPAssetManager/Rules/Filter/RedrawFilterToolbar.js"
                        },
                         {
                            "_Name": "ActivityArea",
                            "_Type": "Control.Type.FormCell.ListPicker",
                            "Caption": "$(L,activity_area)",
                            "AllowMultipleSelection": true,
                            "IsPickerDismissedOnSelection": true,
                            "IsSearchCancelledAfterSelection": true,
                            "Search": {
                                "Enabled": true,
                                "Delay": 500,
                                "MinimumCharacterThreshold": 3,
                                "Placeholder": "$(L,search)",
                                "BarcodeScanner": false
                            },
                            "PickerItems": "/SAPAssetManager/Rules/EWM/WarehouseOrder/SearchFilter/ActivityAreaPickerItems.js",
                            "FilterProperty": "ActivityArea",
                            "OnValueChange": "/SAPAssetManager/Rules/Filter/RedrawFilterToolbar.js"
                        },
                        {
                            "_Name": "SourceBin",
                            "_Type": "Control.Type.FormCell.ListPicker",
                            "Caption": "$(L,source_bin)",
                            "AllowMultipleSelection": true,
                            "IsPickerDismissedOnSelection": true,
                            "IsSearchCancelledAfterSelection": true,
                            "Search": {
                                "Enabled": true,
                                "Delay": 500,
                                "MinimumCharacterThreshold": 3,
                                "Placeholder": "$(L,search)",
                                "BarcodeScanner": false
                            },
                            "PickerItems": "/SAPAssetManager/Rules/EWM/WarehouseOrder/SearchFilter/SourceBinPickerItems.js",
                            "FilterProperty": "SourceBin",
                            "OnValueChange": "/SAPAssetManager/Rules/Filter/RedrawFilterToolbar.js"
                        },
                        {
                            "_Name": "DestinationBin",
                            "_Type": "Control.Type.FormCell.ListPicker",
                            "Caption": "$(L,destination_bin)",
                            "AllowMultipleSelection": true,
                            "IsPickerDismissedOnSelection": true,
                            "IsSearchCancelledAfterSelection": true,
                            "Search": {
                                "Enabled": true,
                                "Delay": 500,
                                "MinimumCharacterThreshold": 3,
                                "Placeholder": "$(L,search)",
                                "BarcodeScanner": false
                            },
                            "PickerItems": "/SAPAssetManager/Rules/EWM/WarehouseOrder/SearchFilter/DestinationBinPickerItems.js",
                            "FilterProperty": "DestinationBin",
                            "OnValueChange": "/SAPAssetManager/Rules/Filter/RedrawFilterToolbar.js"
                        }

                    ]
                }
            ],
            "_Name": "FormCellContainer",
            "_Type": "Control.Type.FormCellContainer"
        }
    ],
    "FioriToolbar": {
        "_Name": "StockSearchFilterFioriToolbar",
        "_Type": "Control.Type.FioriToolbar",
        "Items": [
            {
                "_Type": "Fragment",
                "Fragment": "/SAPAssetManager/Pages/Fragments/Filter/ResetFilterToolbarItem.fragment"
            }
        ]
    },
    "_Type": "Page",
    "_Name": "WarehouseOrderSearchFilterPage",
    "OnLoaded": "/SAPAssetManager/Rules/EWM/WarehouseOrder/SearchFilter/WarehouseOrderSearchFilterOnLoaded.js"
}