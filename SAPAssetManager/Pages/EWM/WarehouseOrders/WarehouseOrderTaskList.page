{
	"Caption": "$(L,emw_wo_tasks)",
	"ActionBar": {
		"Items": [
			{
				"Position": "Right",
				"Icon": "sap-icon://filter",
				"Caption": "/SAPAssetManager/Rules/Filter/FilterButtonCaption.js",
				"OnPress": "/SAPAssetManager/Actions/EWM/WarehouseTasks/WarehouseTaskFilterViewNav.action",
				"_Name": "FilterButton"
			},
			"/SAPAssetManager/Pages/Fragments/SyncButton.fragment"
		]
	},
	"Controls": [
		{
			"Sections": [
				{
					"Header": {
						"Caption": "/SAPAssetManager/Rules/EWM/WarehouseTask/ListView/WarehouseTaskListCaption.js",
						"UseTopPadding": false,
						"Items": [
							{
								"_Type": "SectionHeaderItem.Type.Button",
								"_Name": "SelectItems",
								"Title": "$(L,select_items)",
								"Position": "Right",
								"Visible": "/SAPAssetManager/Rules/EWM/WarehouseTask/ListView/IsSelectionModeEnabled.js",
								"OnPress": "/SAPAssetManager/Rules/EWM/WarehouseTask/ListView/SwitchSelect.js",
								"Styles": {
                                    "Button": "SelectItemsButtonLabelFontSize"
                                }
							},
							{
								"_Type": "SectionHeaderItem.Type.Button",
								"_Name": "SelectAll",
								"Title": "$(L,select_all)",
								"Position": "Right",
								"Visible": false,
								"OnPress": "/SAPAssetManager/Rules/EWM/WarehouseTask/ListView/SwitchSelect.js",
								"Styles": {
                                    "Button": "SelectItemsButtonLabelFontSize"
                                }
							},
							{
								"_Type": "SectionHeaderItem.Type.Button",
								"_Name": "DeselectAll",
								"Title": "$(L,deselect_all)",
								"Position": "Right",
								"Visible": false,
								"OnPress": "/SAPAssetManager/Rules/EWM/WarehouseTask/ListView/SwitchSelect.js",
								"Styles": {
                                    "Button": "SelectItemsButtonLabelFontSize"
                                }
							},
							{
								"_Type": "SectionHeaderItem.Type.Label",
								"_Name": "CaptionLabel",
								"Title": "/SAPAssetManager/Rules/EWM/WarehouseTask/ListView/WarehouseTaskListCaption.js",
								"Position": "Left",
								"Visible": true
							}
                        ]
					},

					"EmptySection": {
						"Caption": "$(L,ewm_no_tasks)"
					},
					"Selection": {	
						"LongPressToEnable": "/SAPAssetManager/Rules/EWM/WarehouseTask/ListView/SelectionModeOnLongPress.js",
						"ExitOnLastDeselect": true			
					},
					"OnSelectionChanged": "/SAPAssetManager/Rules/EWM/WarehouseTask/ListView/OnDocumentSelectedOrUnselected.js",
					"OnSelectionModeChanged": "/SAPAssetManager/Rules/EWM/WarehouseTask/ListView/OnSelectionModeChanged.js",
					"ObjectCell": "/SAPAssetManager/Pages/Fragments/EWM/WarehouseTaskCell.fragment",
					"Search": {
						"Enabled": true,
						"Delay": 500,
						"MinimumCharacterThreshold": 3,
						"Placeholder": "$(L,search)",
						"BarcodeScanner": true
					},
					"Target": {
						"EntitySet": "WarehouseTasks",
						"QueryOptions": "/SAPAssetManager/Rules/EWM/WarehouseTask/ListView/WarehouseTaskListQuery.js",
						"Service": "/SAPAssetManager/Services/AssetManager.service"
					},
					"DataSubscriptions": [
						"WarehouseOrders",
						"WarehouseTasks",
						"WarehouseTaskConfirmations",
						"WarehouseTaskSerialNumber",
						"WarehouseExceptionCodes",
						"WarehouseStorageBins",
						"WarehouseMovementReasons",
						"WarehouseActivityAreas"
						],
					"_Name": "WarehouseTasksSectionObjectTable",
					"_Type": "Section.Type.ObjectTable"
				}
			],
			"FilterFeedbackBar": {
				"_Type": "Control.Type.FilterFeedbackBar",
				"_Name": "FilterFeedback",
				"ShowAllFilters": true,
				"FastFilters": [
					{
						"_Name": "OpenTasks",
						"_Type": "Control.Type.FastFilterItem",
						"FilterType": "Filter",
						"FilterProperty": "",
						"CustomQueryGroup": "TasksFilter",
						"DisplayValue": "/SAPAssetManager/Rules/EWM/WarehouseTask/ListView/WarehouseTaskOpenFilterDisplayValue.js",
						"ReturnValue": "WTStatus eq '' or WTStatus eq 'B'"
					},
					{
						"_Name": "ConfirmedTasks",
						"_Type": "Control.Type.FastFilterItem",
						"FilterType": "Filter",
						"FilterProperty": "",
						"CustomQueryGroup": "TasksFilter",
						"DisplayValue": "/SAPAssetManager/Rules/EWM/WarehouseTask/ListView/WarehouseTaskConfirmedFilterDisplayValue.js",
						"ReturnValue": "WTStatus eq 'C'"
					}
				]
			},
			"Filters": "/SAPAssetManager/Rules/EWM/WarehouseTask/ListView/GetWarehouseTaskPreselectedFilter.js",
			"_Name": "SectionedTable",
			"_Type": "Control.Type.SectionedTable"
		}
	],
	"FioriToolbar": {
        "_Name": "WTFioriToolbar",
        "_Type": "Control.Type.FioriToolbar",
        "Items": [
			{ 
				"_Type": "Fragment",
            	"Fragment": "/SAPAssetManager/Pages/Fragments/EWM/WTConfirmAll.fragment"           
			},
			"/SAPAssetManager/Pages/Fragments/EWM/UnassignToolbarItem.fragment",
			"/SAPAssetManager/Pages/Fragments/EWM/IBDCreateWHTask.fragment"
        ]
    },
	"OnLoaded": "/SAPAssetManager/Rules/EWM/WarehouseTask/ListView/PageOnLoaded.js",
	"OnReturning": "/SAPAssetManager/Rules/EWM/WarehouseTask/ListView/WarehouseOrderTaskListOnReturning.js",
	"_Name": "WarehouseOrderTaskList",
	"_Type": "Page"
}