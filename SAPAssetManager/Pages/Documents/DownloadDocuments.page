			{
				"_Name": "DownloadButton",
				"_Type": "FioriToolbarItem.Type.Button",
				"Title": "$(L, download)",
				"OnPress": "/SAPAssetManager/Rules/Documents/DownloadDocuments/DownloadDocumentsOnDownloadPress.js",
				"ButtonType": "Primary"
