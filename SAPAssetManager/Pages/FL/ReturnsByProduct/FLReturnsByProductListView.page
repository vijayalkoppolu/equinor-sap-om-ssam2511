{
    "Caption": "$(L,fld_return_by_product)",
    "ActionBar": {
        "Items": [
              {
                "Position": "Right",
                "Icon": "$(PLT,sap-icon://filter,sap-icon://filter, '' , sap-icon://filter)",
                "Caption": "/SAPAssetManager/Rules/Filter/FilterButtonCaption.js",
                "OnPress": "/SAPAssetManager/Actions/FL/ReturnsByProduct/FLReturnsByProductFilterViewNav.action",
                "_Name": "FilterButton"
            },
            "/SAPAssetManager/Pages/Fragments/SyncButton.fragment"
        ]
    },
    "Controls": [
        {
            "Sections": [
                {
                    "Header": {
                        "Caption": "",
                        "UseTopPadding": false,
                        "Items": [
							{
								"_Type": "SectionHeaderItem.Type.Button",
								"_Name": "SelectItems",
								"Title": "$(L,select_items)",
								"Position": "Right",
								"Visible": true,
                                "Enabled": "/SAPAssetManager/Rules/FL/ReturnsByProduct/ListView/IsSelectItemsButtonActive.js",
								"OnPress": "/SAPAssetManager/Rules/FL/ReturnsByProduct/ListView/SwitchSelect.js",
                                "Styles": {
                                    "Button": "SelectItemsButtonLabelFontSize"
                                }
							},
							{
								"_Type": "SectionHeaderItem.Type.Button",
								"_Name": "DeselectAll",
								"Title": "$(L,deselect_all)",
								"Position": "Right",
								"Visible": false,
								"OnPress": "/SAPAssetManager/Rules/FL/ReturnsByProduct/ListView/SwitchSelect.js",
                                "Styles": {
                                    "Button": "SelectItemsButtonLabelFontSize"
                                }
							},
							{
								"_Type": "SectionHeaderItem.Type.Button",
								"_Name": "SelectAll",
								"Title": "$(L,select_all)",
								"Position": "Right",
								"Visible": false,
								"OnPress": "/SAPAssetManager/Rules/FL/ReturnsByProduct/ListView/SwitchSelect.js",
                                "Styles": {
                                    "Button": "SelectItemsButtonLabelFontSize"
                                }
							},
                            {
								"_Type": "Fragment",
								"Fragment": "/SAPAssetManager/Pages/Fragments/FL/ListCounterCaption.fragment",
								"Properties": {
									"Title": "/SAPAssetManager/Rules/FL/ReturnsByProduct/ListView/FLProductsCountCaption.js"
								}
							}
                        ]
                    },
                    "EmptySection": {
                        "Caption": "$(L,no_fld_return_by_product)"
                    },
                    "Selection": {
                        "LongPressToEnable": "Multiple",
                        "ExitOnLastDeselect": true,
                        "Mode": "None"
                    },
                    "ObjectCell": "/SAPAssetManager/Pages/Fragments/FL/ProductCell.fragment",
                    "OnSelectionChanged": "/SAPAssetManager/Rules/FL/ReturnsByProduct/OnDocumentSelectedOrUnselected.js",
                    "OnSelectionModeChanged": "/SAPAssetManager/Rules/FL/ReturnsByProduct/ListView/OnProductSelectionModeChanged.js",
                    "Search": {
                        "Enabled": true,
                        "Delay": 500,
                        "MinimumCharacterThreshold": 3,
                        "Placeholder": "$(L,search)",
                        "BarcodeScanner": true
                    },
                    "Target": {
                        "EntitySet": "FldLogsInitRetProducts",
                        "Service": "/SAPAssetManager/Services/AssetManager.service",
                        "QueryOptions": "/SAPAssetManager/Rules/FL/ReturnsByProduct/ReturnsByProductOnLoadQuery.js"
                    },
                    "_Name": "SectionObjectTable",
                    "_Type": "Section.Type.ObjectTable"
                }
            ],
                "FilterFeedbackBar": {
                "_Type": "Control.Type.FilterFeedbackBar",
                "_Name": "FilterFeedback",
                "ShowAllFilters": true,
                "FastFilters": [
                    {
                        "_Name": "AtRemoteStatus",
                        "_Type": "Control.Type.FastFilterItem",
                        "FilterType": "Filter",
                        "FilterProperty": "",
                        "CustomQueryGroup": "FLReturnsByProductStatusesList",
                        "DisplayValue": "/SAPAssetManager/Rules/FL/ReturnsByProduct/FLReturnsByProductFilterCaptionRemote.js",
                        "ReturnValue": "FldLogsReturnStatus eq '10'"

                    },
                    {
                        "_Name": "ReturnScheduledStatus",
                        "_Type": "Control.Type.FastFilterItem",
                        "FilterType": "Filter",
                        "FilterProperty": "",
                        "CustomQueryGroup": "FLReturnsByProductStatusesList",
                        "DisplayValue": "/SAPAssetManager/Rules/FL/ReturnsByProduct/FLReturnsByProductFilterCaptionReturnScheduled.js",
                        "ReturnValue": "FldLogsReturnStatus eq '30'"
                    },
                    {
                        "_Name": "ReadyForDispatchStatus",
                        "_Type": "Control.Type.FastFilterItem",
                        "FilterType": "Filter",
                        "FilterProperty": "",
                        "CustomQueryGroup": "FLReturnsByProductStatusesList",
                        "DisplayValue": "/SAPAssetManager/Rules/FL/ReturnsByProduct/FLReturnsByProductFilterCaptionReadyForDispatch.js",
                        "ReturnValue": "FldLogsReturnStatus eq '40'"
                    },
                    {
                        "_Name": "DispatchedStatus",
                        "_Type": "Control.Type.FastFilterItem",
                        "FilterType": "Filter",
                        "FilterProperty": "",
                        "CustomQueryGroup": "FLReturnsByProductStatusesList",
                        "DisplayValue": "/SAPAssetManager/Rules/FL/ReturnsByProduct/FLReturnsByProductFilterCaptionDispatched.js",
                        "ReturnValue": "FldLogsReturnStatus eq '50'"
                    }
                ]
            },
            "Filters": "/SAPAssetManager/Rules/FL/ReturnsByProduct/GetFLReturnsByProductAtRemotePreSelectedFilter.js",
            "_Name": "SectionedTable",
            "_Type": "Control.Type.SectionedTable"
        }
    ],
    "DataSubscriptions": [
        "FldLogsInitRetProducts",
        "FldLogsReturnStatuses",
        "FldLogsSupprocs",
        "FldLogsRecommendedActions",
        "StorageLocations",
        "FldLogsPlants"
    ],
        "FioriToolbar": {
		"_Name": "FLReturnsByProductListFioriToolbar",
        "_Type": "Control.Type.FioriToolbar",
        "Items": [
            {
                "_Type": "Fragment",
                "Fragment": "/SAPAssetManager/Pages/Fragments/FL/InitiateReturns.fragment"
            },
            {
                "_Type": "Fragment",
                "Fragment": "/SAPAssetManager/Pages/Fragments/FL/FLLoadQuantity.fragment"
            }
        ]
    },
    "_Type": "Page",
    "_Name": "FLReturnsbyProductListViewPage",
    "OnLoaded": "/SAPAssetManager/Rules/FL/Edit/FLListPageWithFilterOnLoaded.js",
    "OnReturning": "/SAPAssetManager/Rules/FL/WorkOrders/ListView/FLWorkOrdersListViewOnReturning.js"
}