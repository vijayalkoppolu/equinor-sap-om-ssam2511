
{
	
	"OnLoaded": "/SAPAssetManager/Rules/Filter/ListPageWithFilterOnLoaded.js",
	"ActionBar": {
		"Items": [
			"/SAPAssetManager/Pages/Fragments/SyncButton.fragment"
		]
	},
	"Controls": [
		{
			"Sections": [
				{
					"Header": {
						"Caption": "",
						"UseTopPadding": false,
						"Items": [
							{
								"_Type": "SectionHeaderItem.Type.Button",
								"_Name": "SelectItems",
								"Title": "$(L,select_items)",
								"Position": "Right",
								"Visible": true,
								"Enabled": "/SAPAssetManager/Rules/FL/WorkOrders/ReservationItems/ListView/IsSelectItemsButtonActive.js",
								"OnPress": "/SAPAssetManager/Rules/FL/WorkOrders/ReservationItems/ListView/SwitchSelect.js",
								"Styles": {
                                    "Button": "SelectItemsButtonLabelFontSize"
                                }
							},
							{
								"_Type": "SectionHeaderItem.Type.Button",
								"_Name": "DeselectAll",
								"Title": "$(L,deselect_all)",
								"Position": "Right",
								"Visible": false,
								"OnPress": "/SAPAssetManager/Rules/FL/WorkOrders/ReservationItems/ListView/SwitchSelect.js",
								"Styles": {
                                    "Button": "SelectItemsButtonLabelFontSize"
                                }
							},
							{
								"_Type": "SectionHeaderItem.Type.Button",
								"_Name": "SelectAll",
								"Title": "$(L,select_all)",
								"Position": "Right",
								"Visible": false,
								"OnPress": "/SAPAssetManager/Rules/FL/WorkOrders/ReservationItems/ListView/SwitchSelect.js",
								"Styles": {
                                    "Button": "SelectItemsButtonLabelFontSize"
                                }
							},
							{
								"_Type": "Fragment",
								"Fragment": "/SAPAssetManager/Pages/Fragments/FL/ListCounterCaption.fragment",
								"Properties": {
									"Title": "/SAPAssetManager/Rules/FL/WorkOrders/ReservationItems/ListView/FLResvItemsCountCaption.js"
								}
							}
                        ]
					},
					"EmptySection": {
						"Caption": "$(L, no_documents_available)"
					},
					"ObjectCell": "/SAPAssetManager/Pages/Fragments/FL/OrderReservationItem.fragment",
					"Selection": {
						"LongPressToEnable": "Multiple",
						"ExitOnLastDeselect": true,
						"Mode": "None"
					},
					"OnSelectionChanged": "/SAPAssetManager/Rules/FL/WorkOrders/ReservationItems/ListView/OnProductSelectedOrUnselected.js",
					"OnSelectionModeChanged": "/SAPAssetManager/Rules/FL/WorkOrders/ReservationItems/ListView/OnProductSelectionModeChanged.js",
					
					"Search": {
						"BarcodeScanner": true,
						"Enabled": true,
						"MinimumCharacterThreshold": 3,
						"Placeholder": "$(L,search)",
						"Delay": 700
					},
					"Target": {
						"EntitySet": "{{#Property:@odata.readLink}}/FldLogsWoResvItem_Nav",
						"QueryOptions": "/SAPAssetManager/Rules/FL/WorkOrders/ReservationItems/ListView/FLResvItemsListViewQuery.js",
						"Service": "/SAPAssetManager/Services/AssetManager.service"
					},
					"DataSubscriptions": [
						"FldLogsWoResvItems"
					],
					"_Name": "SectionObjectTable0",
					"_Type": "Section.Type.ObjectTable"
				}
			],
			"FilterFeedbackBar": {
				"_Type": "Control.Type.FilterFeedbackBar",
				"_Name": "FilterFeedback",
				"ShowAllFilters": true,
				"FastFilters": [
					{  
						"_Name": "Open",
						"_Type": "Control.Type.FastFilterItem",
						"FilterType": "Filter",
						"FilterProperty": "",
						"CustomQueryGroup": "FLOResvItemsList",
						"DisplayValue": "/SAPAssetManager/Rules/FL/WorkOrders/ReservationItems/ListView/FLResvItemsOpenFilterDisplayValue.js",
						"ReturnValue": "Status eq ''"
					},
					{  
						"_Name": "Returned",
						"_Type": "Control.Type.FastFilterItem",
						"FilterType": "Filter",
						"FilterProperty": "",
						"CustomQueryGroup": "FLOResvItemsList",
						"DisplayValue": "/SAPAssetManager/Rules/FL/WorkOrders/ReservationItems/ListView/FLResvItemsReturnFilterDispValue.js",
						"ReturnValue": "Status eq 'R'"
					}
				]
			},
			
			"Filters": "/SAPAssetManager/Rules/FL/WorkOrders/ReservationItems/ListView/GetFLOrderResvItemsPreselectedFilter.js",
			"_Name": "SectionedTable",
			"_Type": "Control.Type.SectionedTable"
		}
	],
	"FioriToolbar": {
		"_Name": "ProductListFioriToolbar",
        "_Type": "Control.Type.FioriToolbar",
		"Items": [
		   { 
			"_Type": "Fragment",
            "Fragment": "/SAPAssetManager/Pages/Fragments/FL/ReturnToStock.fragment"           
			}
		]
	},
	"OnReturning": "/SAPAssetManager/Rules/FL/WorkOrders/ReservationItems/ListView/ListPageWithFilterOnLoaded.js",
	"OnResume": "/SAPAssetManager/Rules/FL/WorkOrders/ReservationItems/ListView/ListPageWithFilterOnLoaded.js",
	"_Name": "FLWOResvItemsList",
	"_Type": "Page"
}