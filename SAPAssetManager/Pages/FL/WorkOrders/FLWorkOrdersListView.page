{
    "Caption": "$(L,fld_return_by_order_list)",
    "ActionBar": {
        "Items": [
            {
                "Position": "Right",
                "Icon": "$(PLT,sap-icon://filter,sap-icon://filter, '' , sap-icon://filter)",
                "Caption": "/SAPAssetManager/Rules/Filter/FilterButtonCaption.js",
                "OnPress": "/SAPAssetManager/Actions/FL/WorkOrders/FLWorkOrdersFilterViewNav.action",
                "_Name": "FilterButton"
            },
            "/SAPAssetManager/Pages/Fragments/SyncButton.fragment"
        ]
    },
    "Controls": [
        {
            "Sections": [
                {
                    "Header": {
                        "Caption": "/SAPAssetManager/Rules/FL/WorkOrders/ListView/WorkOrdersCountCaption.js",
                        "UseTopPadding": false,
                        "Items": [
                            {
								"_Type": "Fragment",
								"Fragment": "/SAPAssetManager/Pages/Fragments/FL/ListCounterCaption.fragment",
								"Properties": {
									"Title": "/SAPAssetManager/Rules/FL/WorkOrders/ListView/WorkOrdersCountCaption.js"
								}
							},
                            {
                                "_Type": "SectionHeaderItem.Type.Button",
                                "_Name": "SelectItems",
                                "Title": "$(L,select_items)",
                                "Position": "Right",
                                "Visible": true,
                                "Enabled": "/SAPAssetManager/Rules/FL/WorkOrders/ListView/IsSelectItemsButtonActive.js",
                                "OnPress": "/SAPAssetManager/Rules/FL/SwitchSelect.js",
                                "Styles": {
                                    "Button": "SelectItemsButtonLabelFontSize"
                                }
                            },
                            {
                                "_Type": "SectionHeaderItem.Type.Button",
                                "_Name": "DeselectAll",
                                "Title": "$(L,deselect_all)",
                                "Position": "Right",
                                "Visible": false,
                                "OnPress": "/SAPAssetManager/Rules/FL/SwitchSelect.js",
                                "Styles": {
                                    "Button": "SelectItemsButtonLabelFontSize"
                                }
                            },
                            {
                                "_Type": "SectionHeaderItem.Type.Button",
                                "_Name": "SelectAll",
                                "Title": "$(L,select_all)",
                                "Position": "Right",
                                "Visible": false,
                                "OnPress": "/SAPAssetManager/Rules/FL/SwitchSelect.js",
                                "Styles": {
                                    "Button": "SelectItemsButtonLabelFontSize"
                                }
                            }]

                    },
                    "EmptySection": {
                        "Caption": "$(L,no_fld_return_by_order_list)"
                    },
                    "Selection": {
                        "LongPressToEnable": "Multiple",
                        "ExitOnLastDeselect": true,
                        "Mode": "None"
                    },
                    "ObjectCell": "/SAPAssetManager/Pages/Fragments/FL/WorkNetworkOrderCell.fragment",
                    "OnSelectionChanged": "/SAPAssetManager/Rules/FL/WorkOrders/ListView/OnDocumentSelectedOrUnselected.js",
                    "Search": {
                        "Enabled": true,
                        "Delay": 500,
                        "MinimumCharacterThreshold": 3,
                        "Placeholder": "$(L,search)",
                        "BarcodeScanner": true
                    },
                    "Target": {
                        "EntitySet": "FldLogsWorkOrders",
                        "Service": "/SAPAssetManager/Services/AssetManager.service",
                        "QueryOptions": "/SAPAssetManager/Rules/FL/WorkOrders/WorkOrdersOnLoadQuery.js"
                    },
                    "_Name": "SectionObjectTable",
                    "_Type": "Section.Type.ObjectTable"
                }
            ],
            "FilterFeedbackBar": {
                "_Type": "Control.Type.FilterFeedbackBar",
                "_Name": "FilterFeedback",
                "ShowAllFilters": true,
                "FastFilters": [
                    {
                        "_Name": "OpenWorkOrders",
                        "_Type": "Control.Type.FastFilterItem",
                        "FilterType": "Filter",
                        "FilterProperty": "",
                        "CustomQueryGroup": "FLWorkOrderList",
                        "DisplayValue": "/SAPAssetManager/Rules/FL/WorkOrders/ListView/WorkOrdersFilterCaptionOpen.js",
                        "ReturnValue": "/SAPAssetManager/Rules/FL/WorkOrders/ListView/GetReturnValueForOpenWorkOrders.js"
                    },
                    {
                        "_Name": "CompletedWorkOrders",
                        "_Type": "Control.Type.FastFilterItem",
                        "FilterType": "Filter",
                        "FilterProperty": "",
                        "CustomQueryGroup": "FLWorkOrderList",
                        "DisplayValue": "/SAPAssetManager/Rules/FL/WorkOrders/ListView/WorkOrdersFilterCaptionCompleted.js",
                        "ReturnValue": "/SAPAssetManager/Rules/FL/WorkOrders/ListView/GetReturnValueForCompletedWorkOrders.js"
                    }
                ]
            },
            "Filters": "/SAPAssetManager/Rules/FL/WorkOrders/ListView/GetFLWorkOrdersPreSelectedFilter.js",
            "_Name": "SectionedTable",
            "_Type": "Control.Type.SectionedTable"
        }
    ],
    "DataSubscriptions": [
        "FldLogsWorkOrders"
    ],
    "FioriToolbar": {
		"_Name": "FLWorkOrderListFioriToolbar",
        "_Type": "Control.Type.FioriToolbar",
		"Items": [
			{
				"_Type": "Fragment",
				"Fragment": "/SAPAssetManager/Pages/Fragments/FL/FLDiscardButton.fragment",
				"Properties": {
					"_Name": "FLWorkOrderDiscardItem"
			}
			}
		]
    },
    "_Type": "Page",
    "_Name": "FLWorkOrdersListViewPage",
    "OnLoaded": "/SAPAssetManager/Rules/Filter/ListPageWithFilterOnLoaded.js",
    "OnReturning": "/SAPAssetManager/Rules/FL/WorkOrders/ListView/FLWorkOrdersListViewOnReturning.js"
}
