{
    "OnActivityBackPressed": "/SAPAssetManager/Rules/WorkOrders/CreateUpdate/CancelCheckOnBackNavAndroid.js",
    "ActionBar": {
        "Items": [
            {
                "Position": "Left",
                "SystemItem": "Cancel",
                "Caption": "$(L,cancel)",
                "OnPress": "/SAPAssetManager/Actions/Page/CancelPage.action"
            },
            {
                "Position": "Right",
                "SystemItem": "/SAPAssetManager/Rules/Filter/FilterSystemItem.js",
                "Text": "/SAPAssetManager/Rules/Filter/FilterText.js",
                "OnPress": "/SAPAssetManager/Rules/Filter/ApplyFilterAndSave.js"
            }
        ]
    },
    "Caption":"$(L,filter)",
    "Result": "/SAPAssetManager/Rules/FL/BulkUpdate/BulkEditsSearchFilterResults.js",
    "Controls":
    [{
        "Sections":
        [
            {
                "Caption": "$(L,filter_by)",
                "Controls":
                [
                {
                    "_Name": "ProductIDFilter",
                    "_Type": "Control.Type.FormCell.ListPicker",
                    "Caption": "$(L,product)",
                    "AllowMultipleSelection": true,
                    "Search": {
                        "Enabled": true,
                        "Delay": 500,
                        "MinimumCharacterThreshold": 3,
                        "Placeholder": "$(L,search)",
                        "BarcodeScanner": true
                    },
                    "PickerItems": "/SAPAssetManager/Rules/FL/BulkUpdate/ProductPickerItems.js",
                    "FilterProperty": "Material",
                    "OnValueChange": "/SAPAssetManager/Rules/Filter/RedrawFilterToolbar.js"
                },
                {
                    "_Name": "ReferenceDocFilter",
                    "_Type": "Control.Type.FormCell.ListPicker",
                    "Caption": "$(L,reference_doc_number)",
                    "AllowMultipleSelection": true,
                    "Search": {
                        "Enabled": true,
                        "Delay": 500,
                        "MinimumCharacterThreshold": 3,
                        "Placeholder": "$(L,search)",
                        "BarcodeScanner": true
                    },
                    "PickerItems": "/SAPAssetManager/Rules/FL/BulkUpdate/ReferenceDocPickerItems.js",
                    "FilterProperty": "ReferenceDocNumber",
                    "OnValueChange": "/SAPAssetManager/Rules/Filter/RedrawFilterToolbar.js"
                },
                 {
                    "_Name": "ItemStatusFilter",
                    "_Type": "Control.Type.FormCell.ListPicker",
                    "Caption": "$(L,item_status)",
                    "AllowMultipleSelection": true,
                    "Search": {
                        "Enabled": true,
                        "Delay": 500,
                        "MinimumCharacterThreshold": 3,
                        "Placeholder": "$(L,search)",
                        "BarcodeScanner": true
                    },
                     "PickerItems": {
                        "DisplayValue": "{{#Property:FldContainerItemStatusDesc}}",
                        "ReturnValue": "{FldContainerItemStatus}",
                        "Target": {
                            "EntitySet": "FldLogsContainerItemStatuses",
                            "Service": "/SAPAssetManager/Services/AssetManager.service"
                        }
                    },
                    "FilterProperty": "ContainerItemStatus",
                    "OnValueChange": "/SAPAssetManager/Rules/Filter/RedrawFilterToolbar.js"
                },
                {
                    "_Name": "HandlingDecisionsFilter",
                    "_Type": "Control.Type.FormCell.ListPicker",
            
                    "AllowMultipleSelection": true,
                    "Search": {
                        "Enabled": true,
                        "Delay": 500,
                        "MinimumCharacterThreshold": 3,
                        "Placeholder": "$(L,search)",
                        "BarcodeScanner": true
                    },
                
                    "Caption": "$(L,handling_decision)",
                    "PickerItems": {
                        "DisplayValue": "{{#Property:Description}}",
                        "ReturnValue": "{HandlingDecision}",
                        "Target": {
                            "EntitySet": "FldLogsHandlingDecisions",
                            "Service": "/SAPAssetManager/Services/AssetManager.service",
                            "QueryOptions": "$orderby=HandlingDecision,ProcessType"
                        }
                    },
                    "FilterProperty": "HandlingDecision",
                    "OnValueChange": "/SAPAssetManager/Rules/Filter/RedrawFilterToolbar.js"
                },
                {
                    "_Name": "StorageLocationFilter",
                    "_Type": "Control.Type.FormCell.ListPicker",
            
                    "AllowMultipleSelection": true,
                    "Search": {
                        "Enabled": true,
                        "Delay": 500,
                        "MinimumCharacterThreshold": 3,
                        "Placeholder": "$(L,search)",
                        "BarcodeScanner": true
                    },
                
                    "Caption": "$(L,storage_location)",
                    "PickerItems": {
                        "DisplayValue": "{{#Property:StorageLocation}}-{{#Property:StorageLocationDesc}}",
                        "ReturnValue": "{StorageLocation}",
                        "Target": {
                        "EntitySet": "StorageLocations",
                        "Service": "/SAPAssetManager/Services/AssetManager.service",
                        "QueryOptions": "/SAPAssetManager/Rules/Inventory/Fetch/StorageLocationQueryOptions.js"
                        }
                    },
                    "FilterProperty": "DestStorLocID",
                    "OnValueChange": "/SAPAssetManager/Rules/Filter/RedrawFilterToolbar.js"
                }
                ]
            }
        ],
        "_Name":"FormCellContainer",
        "_Type":"Control.Type.FormCellContainer"
    }],
    "FioriToolbar": {
        "_Name": "ContainersSearchFilterFioriToolbar",
        "_Type": "Control.Type.FioriToolbar",
        "Items": [
            {
				"_Type": "Fragment",
                "Fragment": "/SAPAssetManager/Pages/Fragments/Filter/ResetFilterToolbarItem.fragment"
			}
        ]
    },
    "_Type":"Page",
    "_Name":"BulkFLEditFilterPage",
    "OnLoaded": "/SAPAssetManager/Rules/FL/Containers/ListView/ContainersSearchFilterOnLoaded.js"
}