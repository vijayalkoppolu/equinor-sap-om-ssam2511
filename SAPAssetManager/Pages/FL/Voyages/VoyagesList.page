{
"Caption": "$(L,fld_voyages)",
"ActionBar": {
		"Items": [
            {
                "Position": "Right",
                "Icon": "$(PLT,sap-icon://filter,sap-icon://filter, '' , sap-icon://filter)",
                "Caption": "/SAPAssetManager/Rules/Filter/FilterButtonCaption.js",
                "OnPress": "/SAPAssetManager/Actions/FL/Voyages/VoyagesFilterViewNav.action",
				"_Name": "FilterButton"
            },
			"/SAPAssetManager/Pages/Fragments/SyncButton.fragment"
		]
	},
"Controls": [
		{
			"Sections": [
				{
					"EmptySection": {
						"Caption": "$(L,no_fld_voyages)"
					},
					"Header": {
						"Caption": "/SAPAssetManager/Rules/FL/Voyages/VoyagesListCaption.js",
						"UseTopPadding": false,
						"Items": [
							{
								"_Type": "Fragment",
								"Fragment": "/SAPAssetManager/Pages/Fragments/FL/ListCounterCaption.fragment"
							},
							{
								"_Type": "SectionHeaderItem.Type.Button",
                                "_Name": "SelectItems",
                                "Title": "$(L,select_items)",
                                "Position": "Right",
                                "Visible": true,
                                "Enabled": "/SAPAssetManager/Rules/FL/Voyages/ListView/IsSelectItemsButtonActive.js",
                                "OnPress": "/SAPAssetManager/Rules/FL/SwitchSelect.js",
								"Styles": {
                                    "Button": "SelectItemsButtonLabelFontSize"
                                }
							},
							{
								"_Type": "SectionHeaderItem.Type.Button",
								"_Name": "DeselectAll",
								"Title": "$(L,deselect_all)",
								"Position": "Right",
								"Visible": false,
								"OnPress": "/SAPAssetManager/Rules/FL/SwitchSelect.js",
								"Styles": {
                                    "Button": "SelectItemsButtonLabelFontSize"
                                }
							},
							{
								"_Type": "SectionHeaderItem.Type.Button",
								"_Name": "SelectAll",
								"Title": "$(L,select_all)",
								"Position": "Right",
								"Visible": false,
								"OnPress": "/SAPAssetManager/Rules/FL/SwitchSelect.js",
								"Styles": {
                                    "Button": "SelectItemsButtonLabelFontSize"
                                }
							}
						]
					},
					"Selection": {
					"LongPressToEnable": "Multiple",
					"ExitOnLastDeselect": true,
					"Mode": "None"
					},
					"OnSelectionChanged": "/SAPAssetManager/Rules/FL/Voyages/OnDocumentSelectedOrUnselected.js",
					"ObjectCell": "/SAPAssetManager/Pages/Fragments/FL/VoyageCell.fragment",
					"Search": {
						"Enabled" : true,
                        "Delay": 500,
                        "MinimumCharacterThreshold": 3,
                        "Placeholder": "$(L,search)",
                        "BarcodeScanner": true
					},
					"Target": {
						"EntitySet": "FldLogsVoyages",
						"QueryOptions":"/SAPAssetManager/Rules/FL/Voyages/VoyagesOnLoadQuery.js",
						"Service": "/SAPAssetManager/Services/AssetManager.service"
					},
					"_Name": "SectionObjectTable",
					"_Type": "Section.Type.ObjectTable"
				}
			],
			"FilterFeedbackBar": {
				"_Type": "Control.Type.FilterFeedbackBar",
				"_Name": "FilterFeedback",
				"ShowAllFilters": true,
				"FastFilters": [
	        		{ 
						"_Name": "InTransitItems",
						"_Type": "Control.Type.FastFilterItem",
						"FilterType": "Filter",
						"FilterProperty": "",
						"CustomQueryGroup": "VoyageList",
						"DisplayValue": "/SAPAssetManager/Rules/FL/Voyages/VoyagesFilterCaptionInTransit.js",
						"ReturnValue": "/SAPAssetManager/Rules/FL/Voyages/GetReturnValueforInTransit.js"
            		},
            		{	 
						"_Name": "ArrivedOrCompletedItems",
						"_Type": "Control.Type.FastFilterItem",
						"FilterType": "Filter",
						"FilterProperty": "",
						"CustomQueryGroup": "VoyageList",
						"DisplayValue": "/SAPAssetManager/Rules/FL/Voyages/VoyagesFilterCaptionArrivedCompleted.js",
						"ReturnValue": "/SAPAssetManager/Rules/FL/Voyages/GetReturnValueforArrivedCompleted.js"
            		}
				]
			},
			"Filters": "/SAPAssetManager/Rules/FL/Voyages/GetVoyagesPreSelectedFilter.js",
			"_Name": "SectionedTable",
			"_Type": "Control.Type.SectionedTable"
		}
],
		"FioriToolbar": {
		"_Name": "VoyageListFioriToolbar",
        "_Type": "Control.Type.FioriToolbar",
		"Items": [
			{
				"_Type": "Fragment",
				"Fragment": "/SAPAssetManager/Pages/Fragments/FL/FLDiscardButton.fragment",
				"Properties": {
					"_Name": "FLVoyageDiscardItem"
			}
			}
		]
    },
	"DataSubscriptions" : [
    "FldLogsVoyages"
],
	"_Name": "VoyagesListPage",
	"_Type": "Page",
	"OnLoaded": "/SAPAssetManager/Rules/Filter/ListPageWithFilterOnLoaded.js"
}
