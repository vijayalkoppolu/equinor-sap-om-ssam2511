{
"Caption": "$(L,fld_container_items)",
"ActionBar": 
{ 
	"Items": [
			"/SAPAssetManager/Pages/Fragments/SyncButton.fragment"
		] 
},
"Controls": [
		{
			"Sections": [
				{
					"EmptySection": {
						"Caption": "$(L,no_fld_container_items)"
					},
					"Header": {
						"Caption": "/SAPAssetManager/Rules/FL/ContainerItems/ContainerItemsCountCaption.js",
						"UseTopPadding": false,
						"Items": [
							{
								"_Type": "Fragment",
								"Fragment": "/SAPAssetManager/Pages/Fragments/FL/ListCounterCaption.fragment",
								"Properties": {
									"Title": "/SAPAssetManager/Rules/FL/ContainerItems/ContainerItemsCountCaption.js"
								}
							},
                            {
                                "_Type": "SectionHeaderItem.Type.Button",
                                "_Name": "SelectItems",
                                "Title": "$(L,select_items)",
                                "Position": "Right",
                                "Visible": true,
                                "Enabled": "/SAPAssetManager/Rules/FL/ContainerItems/ListView/IsSelectItemsButtonActive.js",
                                "OnPress": "/SAPAssetManager/Rules/FL/SwitchSelect.js",
								"Styles": {
                                    "Button": "SelectItemsButtonLabelFontSize"
                                }
                            },
							{
                                "_Type": "SectionHeaderItem.Type.Button",
                                "_Name": "DeselectAll",
                                "Title": "$(L,deselect_all)",
                                "Position": "Right",
                                "Visible": false,
                                "OnPress": "/SAPAssetManager/Rules/FL/SwitchSelect.js",
								"Styles": {
                                    "Button": "SelectItemsButtonLabelFontSize"
                                }
                            },
                            {
                                "_Type": "SectionHeaderItem.Type.Button",
                                "_Name": "SelectAll",
                                "Title": "$(L,select_all)",
                                "Position": "Right",
                                "Visible": false,
                                "OnPress": "/SAPAssetManager/Rules/FL/SwitchSelect.js",
								"Styles": {
                                    "Button": "SelectItemsButtonLabelFontSize"
                                }
                            }
						]
					},
					"Selection": {
						"LongPressToEnable": "Multiple",
						"ExitOnLastDeselect": true,
						"Mode": "None"
					},
					"OnSelectionChanged": "/SAPAssetManager/Rules/FL/ContainerItems/OnDocumentSelectedOrUnselected.js",
					"ObjectCell": {
       				    "_Type": "Fragment",
        				"Fragment": "/SAPAssetManager/Pages/Fragments/FL/ContainerItemCell.fragment",
        				"Properties": {
          					"OnAccessoryButtonPress": "/SAPAssetManager/Rules/FL/ContainerItems/EditFLDocumentNav.js",
							"AccessoryButtonIcon": "/SAPAssetManager/Rules/FL/ContainerItems/IsEditAllowed.js"
        				}
   					 },
					"Search": {
						"Enabled" : true,
                        "Delay": 500,
                        "MinimumCharacterThreshold": 3,
                        "Placeholder": "$(L,search)",
                        "BarcodeScanner": true
					},
					"Target": {
						"EntitySet": "FldLogsContainerItems",
						"QueryOptions":"/SAPAssetManager/Rules/FL/ContainerItems/ContainerItemsListQueryOptions.js",
						"Service": "/SAPAssetManager/Services/AssetManager.service"
					},
					"_Name": "ContainerItemsSectionObjectTable",
					"_Type": "Section.Type.ObjectTable",
					"DataSubscriptions": [
						"ContainerItems"
					]
				}
			],
			"FilterFeedbackBar": { 
				"_Type": "Control.Type.FilterFeedbackBar",
				"_Name": "FilterFeedback",
				"ShowAllFilters": true,
				"FastFilters": [
					{ 
						"_Name": "OpenContainerItems",
						"_Type": "Control.Type.FastFilterItem",
						"FilterType": "Filter",
						"CustomQueryGroup": "ContainerItemsList",
						"DisplayValue": "/SAPAssetManager/Rules/FL/ContainerItems/ContainerItemsFilterCaptionOpen.js",
						"ReturnValue": "/SAPAssetManager/Rules/FL/ContainerItems/ContainerItemsReturnValueFilterOpen.js"
					},
					{ 
						"_Name": "ReceivedContainerItems",
						"_Type": "Control.Type.FastFilterItem",
						"FilterType": "Filter",
						"CustomQueryGroup": "ContainerItemsList",
						"DisplayValue": "/SAPAssetManager/Rules/FL/ContainerItems/ContainerItemsFilterCaptionReceived.js",
						"ReturnValue": "/SAPAssetManager/Rules/FL/ContainerItems/ContainerItemsReturnValueFilterReceived.js"
					},
					{
						"_Name": "NotFoundContainerItems",
						"_Type": "Control.Type.FastFilterItem",
						"FilterType": "Filter",
						"CustomQueryGroup": "ContainerItemsList",
						"DisplayValue": "/SAPAssetManager/Rules/FL/ContainerItems/ContainerItemsFilterCaptionNotFound.js",
						"ReturnValue": "/SAPAssetManager/Rules/FL/ContainerItems/ContainerItemsReturnValueFilterNotFound.js"
					}
				]
			},
			"Filters": "/SAPAssetManager/Rules/FL/ContainerItems/GetContainerItemsPreSelectedFilter.js",
			"_Name": "SectionedTable",
			"_Type": "Control.Type.SectionedTable",
			"DataSubscriptions": ["FldLogsContainerItems"]
		}
	],
	    "FioriToolbar": {
        "_Name": "ContainerItemsFioriToolbar",
        "_Type": "Control.Type.FioriToolbar",
        "Items": [
			{ 
			"_Type": "Fragment",
            "Fragment": "/SAPAssetManager/Pages/Fragments/FL/ReceiveAll.fragment"           
			},
			{ 
			"_Type": "Fragment",
            "Fragment": "/SAPAssetManager/Pages/Fragments/FL/ReceiveItem.fragment"           
			},
			{ 
			"_Type": "Fragment",
            "Fragment": "/SAPAssetManager/Pages/Fragments/FL/NotFoundItem.fragment"           
			},
			{
			"_Type": "Fragment",
            "Fragment": "/SAPAssetManager/Pages/Fragments/FL/EditAll.fragment"
			}  
        ]
    },
	"_Name": "ContainerItemsListPage",
	"_Type": "Page",
	"OnLoaded": "/SAPAssetManager/Rules/Filter/ListPageWithFilterOnLoaded.js",
	"OnReturning": "/SAPAssetManager/Rules/FL/ContainerItems/RedrawFilter.js"
}