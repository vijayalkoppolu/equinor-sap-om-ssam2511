{
    "Caption": "$(L, fld_container_details)",
    "ActionBar": {
        "Items": [
            "/SAPAssetManager/Pages/Fragments/SyncButton.fragment"
        ]
    },
    "Controls": [{
            "Sections": [{
                    "ObjectHeader": {
                        "HeadlineText": "$(DV, '{FldLogsContainerID}', '-')",
                        "StatusText": "$(DV, '{FldLogsCtnPkgPackingStatus_Nav/CtnPkgPackingStatusText}', '-')",
                        "SubstatusText": "/SAPAssetManager/Rules/FL/PackedPackages/FormatMetricsSubstatusText.js",
                        "Subhead": "$(DV, '{FldLogsContainerCategoryText}', '-')",
                        "Description": "/SAPAssetManager/Rules/FL/PackedPackages/PackedPackageDescription.js",
                        "Tags": "/SAPAssetManager/Rules/FL/ReadyToPack/FLReadyToPackTags.js",
                        "DetailImage": "sap-icon://dimension",
                        "Styles": {
                                    "DetailImage": "FLContainerIcon"
                                    },
                        "DetailImageIsCircular": false
                    },
                    "_Type": "Section.Type.ObjectHeader",
                    "_Name": "ObjectHeaderSectionTable"
                }],
            "_Name": "SectionedTable",
            "_Type": "Control.Type.SectionedTable"
        },
        {
            "Items": [  {
                "_Name": "HeaderListTab",
                "Caption": "$(L, header)",
                "PageToOpen" : "/SAPAssetManager/Pages/FL/PackContainers/PackedContainers/PackedContainersHeader.page",
                "PageMetadata": "/SAPAssetManager/Rules/FL/PackedContainers/FLPackedContainersHeaderPageMetadata.js",
                "_Type": "Control.Type.TabItem"
            },
            {
                "_Name": "ContainerItemsListTab",
                "Caption": "$(L, fld_container_items)",
                "PageToOpen" : "/SAPAssetManager/Pages/FL/PackContainers/PackedContainers/FLPackedContainerItemsList.page",
                "_Type": "Control.Type.TabItem"
            },
            {
                "_Name": "PackageListTab",
                "Caption": "$(L, fld_packages)",
                "PageToOpen" : "/SAPAssetManager/Pages/FL/PackContainers/PackedContainers/FLPackagesList.page",
                "_Type": "Control.Type.TabItem"
            },
            {
                "_Name": "VoyagesListTab",
                "Caption": "$(L, fld_voyages)",
                "PageToOpen" : "/SAPAssetManager/Pages/FL/PackContainers/PackedContainers/PackedContainersVoyagesPage.page",
                "_Type": "Control.Type.TabItem"
            }],
            "Position": "Top",
            "_Type": "Control.Type.Tabs",
            "_Name": "TabsControl"
        }
    ],
    "_Name": "FLPackedContainersDetailsPage",
    "_Type": "Page"
}
