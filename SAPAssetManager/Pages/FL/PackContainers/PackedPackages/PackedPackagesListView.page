{
    "Caption": "$(L,ready_to_pack)",
    "ActionBar": {
        "Items": [
            "/SAPAssetManager/Pages/Fragments/SyncButton.fragment"
        ]
    },
    "Controls": [
        {
            "Sections": [
                {
                    "Header": {
                        "UseTopPadding": false,
                        "Caption": "/SAPAssetManager/Rules/FL/PackedPackages/PackedPackagesCountCaption.js",
                        "Items": [
							{
								"_Type": "SectionHeaderItem.Type.Button",
								"_Name": "SelectItems",
								"Title": "$(L,select_items)",
								"Position": "Right",
								"Visible": true,
                                "Enabled": "/SAPAssetManager/Rules/FL/PackedPackages/ListView/IsSelectItemsButtonActive.js",
								"OnPress": "/SAPAssetManager/Rules/FL/ReadyToPack/SwitchSelect.js",
                                "Styles": {
                                    "Button": "SelectItemsButtonLabelFontSize"
                                }
							},
							{
								"_Type": "SectionHeaderItem.Type.Button",
								"_Name": "DeselectAll",
								"Title": "$(L,deselect_all)",
								"Position": "Right",
								"Visible": false,
								"OnPress": "/SAPAssetManager/Rules/FL/ReadyToPack/SwitchSelect.js",
                                "Styles": {
                                    "Button": "SelectItemsButtonLabelFontSize"
                                }
							},
							{
								"_Type": "SectionHeaderItem.Type.Button",
								"_Name": "SelectAll",
								"Title": "$(L,select_all)",
								"Position": "Right",
								"Visible": false,
								"OnPress": "/SAPAssetManager/Rules/FL/ReadyToPack/SwitchSelect.js",
                                "Styles": {
                                    "Button": "SelectItemsButtonLabelFontSize"
                                }
							},
                            {
								"_Type": "Fragment",
								"Fragment": "/SAPAssetManager/Pages/Fragments/FL/ListCounterCaption.fragment",
								"Properties": {
									"Title": "/SAPAssetManager/Rules/FL/PackedPackages/PackedPackagesCountCaption.js"
								}
							}
                        ]
                    },
                    "EmptySection": {
                        "Caption": "$(L,fld_no_packed_packages)"
                    },
                    "Selection": {
                        "LongPressToEnable": "Multiple",
                        "ExitOnLastDeselect": true,
                        "Mode": "None"
                    },
                    "OnSelectionChanged": "/SAPAssetManager/Rules/FL/PackedPackages/OnDocumentSelectedOrUnselected.js",
                    "ObjectCell": "/SAPAssetManager/Pages/Fragments/FL/PackedPackagesCell.fragment",
                    "Search": {
                        "Enabled": true,
                        "Delay": 500,
                        "MinimumCharacterThreshold": 3,
                        "Placeholder": "$(L,search)",
                        "BarcodeScanner": true
                    },
                    "Target": {
                        "EntitySet": "FldLogsPackCtnPkdPkgs",
                        "Service": "/SAPAssetManager/Services/AssetManager.service",
                        "QueryOptions": "/SAPAssetManager/Rules/FL/PackedPackages/PackedPackagesOnLoadQuery.js"
                    },
                    "_Name": "SectionObjectTable",
                    "_Type": "Section.Type.ObjectTable"
                }
            ],

                "FilterFeedbackBar": {
                "_Type": "Control.Type.FilterFeedbackBar",
                "_Name": "FilterFeedback",
                "ShowAllFilters": true,
                "FastFilters": [
                    {
                        "_Name": "InPackingStatus",
                        "_Type": "Control.Type.FastFilterItem",
                        "FilterType": "Filter",
                        "FilterProperty": "",
                        "CustomQueryGroup": "FLPackedPackagesStatusesList",
                        "DisplayValue": "/SAPAssetManager/Rules/FL/PackedPackages/FLPackedPackageCaptionInPacking.js",
                        "ReturnValue": "FldLogsCtnPackgStsCode eq '10'"
                    },
                    {
                        "_Name": "NotStartedStatus",
                        "_Type": "Control.Type.FastFilterItem",
                        "FilterType": "Filter",
                        "FilterProperty": "",
                        "CustomQueryGroup": "FLPackedPackagesTransportStatusesList",
                        "DisplayValue": "/SAPAssetManager/Rules/FL/PackedPackages/FLPackedPackagesFilterCaptionNotStarted.js",
                        "ReturnValue": "FldLogsCtnIntTranspStsCode eq '10'"
                    },
                    {
                        "_Name": "ReadyForDispatchStatus",
                        "_Type": "Control.Type.FastFilterItem",
                        "FilterType": "Filter",
                        "FilterProperty": "",
                        "CustomQueryGroup": "FLPackedPackagesTransportStatusesList",
                        "DisplayValue": "/SAPAssetManager/Rules/FL/PackedPackages/FLPackedPackagesFilterCaptionReadyForDispatch.js",
                        "ReturnValue": "FldLogsCtnIntTranspStsCode eq '17'"
                    },
                    {
                        "_Name": "DispatchedStatus",
                        "_Type": "Control.Type.FastFilterItem",
                        "FilterType": "Filter",
                        "FilterProperty": "",
                        "CustomQueryGroup": "FLPackedPackagesTransportStatusesList",
                        "DisplayValue": "/SAPAssetManager/Rules/FL/PackedPackages/FLPackedPackagesFilterCaptionDispatched.js",
                        "ReturnValue": "FldLogsCtnIntTranspStsCode eq '20'"
                    }
                ]
            },
            "Filters": "/SAPAssetManager/Rules/FL/PackedPackages/GetFLPackedPackagesInPackingPreSelectedFilter.js",
            "_Name": "SectionedTable",
            "_Type": "Control.Type.SectionedTable"
        }
    ],
    "DataSubscriptions": [
        "FldLogsPackCtnPkdPkgs"
    ],
    "FioriToolbar": {
		"_Name": "PackedPackagesFioriToolbar",
        "_Type": "Control.Type.FioriToolbar",
        "Items": [
            {
                "_Type": "Fragment",
                "Fragment": "/SAPAssetManager/Pages/Fragments/FL/PackedPackagesEditAll.fragment"
            },
            {
                "_Type": "Fragment",
                "Fragment": "/SAPAssetManager/Pages/Fragments/FL/PackedPackagesAssgnContBtn.fragment"
            }, 
            {
				"_Type": "Fragment",
				 "Fragment": "/SAPAssetManager/Pages/Fragments/FL/GoodsIssue.fragment",
				 "Properties": {
				 "_Name": "FLPackedPackagesGoodsIssueItem",
				 "OnPress": "/SAPAssetManager/Rules/FL/ReadyToPack/ReadyToPackGoodsIssue.js"
				}
			},
            {
                "_Type": "Fragment",
                "Fragment": "/SAPAssetManager/Pages/Fragments/FL/Dispatched.fragment",
                "Properties": {
                "_Name": "FLPackedPackagesDispatchedItem",
                "OnPress": "/SAPAssetManager/Rules/FL/ReadyToPack/ReadyToPackDispatched.js"
                }
            },
            {
                "_Type": "Fragment",
                "Fragment": "/SAPAssetManager/Pages/Fragments/FL/AssignToVoyageButton.fragment",
                "Properties": {
                "_Name": "FLPackedPackagesAssignToVoyageItem",
                "OnPress": "/SAPAssetManager/Rules/FL/Voyages/AssignToVoyage/AssignToVoyageNav.js"
                }
            }
        ]
    },
    "OnReturning": "/SAPAssetManager/Rules/FL/PackedPackages/FLPackedPackagesListViewOnReturning.js",
    "OnResume": "/SAPAssetManager/Rules/FL/PackedPackages/FLPackedPackagesListViewOnReturning.js",
    "_Type": "Page",
    "_Name": "PackedPackagesListViewPage"
}
