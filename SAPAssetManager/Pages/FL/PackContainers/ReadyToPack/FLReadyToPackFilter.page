
{
 
  "ActionBar": {
    "Items": [
      {
        "Position": "Left",
        "SystemItem": "Cancel",
        "Caption": "$(L,cancel)",
        "OnPress": "/SAPAssetManager/Actions/Page/CancelPage.action"
      },
      {
        "Position": "Right",
        "SystemItem": "/SAPAssetManager/Rules/Filter/FilterSystemItem.js",
        "Text": "/SAPAssetManager/Rules/Filter/FilterText.js",
        "OnPress": "/SAPAssetManager/Rules/Filter/ApplyFilterAndSave.js"
      }
    ]
  },
  "Caption": "$(L,filter)",
  "Result": "/SAPAssetManager/Rules/FL/ReadyToPack/FLReadyToPackFilterResults.js",
  "Controls": [
    {
      "Sections": [
        {
          "Controls": [
            {
              "_Type": "Fragment",
              "Fragment": "/SAPAssetManager/Pages/Fragments/Filter/MultiSortByFilter.fragment",
              "Properties": {
                "Items": [
                  {
                    "ReturnValue": "FldLogsContainerID",
                    "DisplayValue": "$(L,fl_container_id)"
                  }
                ]
              }
            }
          ]
        },
        {
          "Caption": "$(L,filter_by)",
          "Controls": [
            {
              "_Type": "Control.Type.FormCell.SimpleProperty",
              "_Name": "DeliveryDocument",
              "AlternateInput": "Barcode",
              "FilterProperty": "DeliveryDocument",
              "Caption": "$(L,delivery_document)"
            },
            {
              "_Type": "Control.Type.FormCell.SimpleProperty",
              "_Name": "ProductDescription",
              "AlternateInput": "Barcode",
              "FilterProperty": "MaterialName",
              "Caption": "$(L,product)"
            },
            {
              "_Type": "Control.Type.FormCell.SimpleProperty",
              "_Name": "Product",
              "AlternateInput": "Barcode",
              "FilterProperty": "Material",
              "Caption": "$(L,product_id_label)"
            },
            {
              "_Name": "DestinationPlantFilter",
              "_Type": "Control.Type.FormCell.ListPicker",
              "Caption": "$(L,destination_plant)",
              "AllowMultipleSelection": false,
              "IsPickerDismissedOnSelection": true,
              "IsSearchCancelledAfterSelection": true,
              "Search": {
                "Enabled": true,
                "Delay": 500,
                "MinimumCharacterThreshold": 3,
                "Placeholder": "$(L,search)",
                "BarcodeScanner": true
              },
              "FilterProperty": "FldLogsDestPlnt",
              "PickerItems": {
                                "DisplayValue": "{{#Property:Plant}} - {{#Property:PlantName}}",
                                "ReturnValue": "{Plant}",
                                "Target": {
                                    "EntitySet": "FldLogsPlants",
                                    "Service": "/SAPAssetManager/Services/AssetManager.service",
                                    "QueryOptions": "$orderby=Plant"
                                }
                            },
              "OnValueChange": "/SAPAssetManager/Rules/Filter/RedrawFilterToolbar.js"
            },
            {
              "_Name": "SourcePlantFilter",
              "_Type": "Control.Type.FormCell.ListPicker",
              "Caption": "$(L,fld_source_plant)",
              "AllowMultipleSelection": false,
              "IsPickerDismissedOnSelection": true,
              "IsSearchCancelledAfterSelection": true,
              "Search": {
                "Enabled": true,
                "Delay": 500,
                "MinimumCharacterThreshold": 3,
                "Placeholder": "$(L,search)",
                "BarcodeScanner": true
              },
              "FilterProperty": "FldLogsSrcePlnt",
              "PickerItems": {
                                "DisplayValue": "{{#Property:Plant}} - {{#Property:PlantName}}",
                                "ReturnValue": "{Plant}",
                                "Target": {
                                    "EntitySet": "FldLogsPlants",
                                    "Service": "/SAPAssetManager/Services/AssetManager.service",
                                    "QueryOptions": "$orderby=Plant"
                                }
                            },
              "OnValueChange": "/SAPAssetManager/Rules/Filter/RedrawFilterToolbar.js"
            },
            {
              "_Name": "PackingStatusFilter",
              "_Type": "Control.Type.FormCell.ListPicker",
              "Caption": "$(L,packing_status)",
              "AllowMultipleSelection": false,
              "IsPickerDismissedOnSelection": true,
              "IsSearchCancelledAfterSelection": true,
              "FilterProperty": "FldLogsShptItmStsCode",
              "Search": {
                          "Enabled": true,
                           "Delay": 500,
                           "MinimumCharacterThreshold": 3,
                           "Placeholder": "$(L,search)",
                           "BarcodeScanner": true
                          },
              "PickerItems": {
                  "DisplayValue": "{{#Property:FldLogsShptItmStsCode}} - {{#Property:FldLogsShptItmStsText}}",
                  "ReturnValue": "{FldLogsShptItmStsCode}",
                  "Target": {
                      "EntitySet": "FldLogsPackCtnItemStatuses",
                      "Service": "/SAPAssetManager/Services/AssetManager.service",
                      "QueryOptions": "$orderby=FldLogsShptItmStsCode"
                  }
                }
            },
            {
              "_Name": "ItemStatusFilter",
              "_Type": "Control.Type.FormCell.ListPicker",
              "Caption": "$(L,item_status)",
              "AllowMultipleSelection": false,
              "IsPickerDismissedOnSelection": true,
              "IsSearchCancelledAfterSelection": true,
              "Search": {
                "Enabled": true,
                "Delay": 500,
                "MinimumCharacterThreshold": 3,
                "Placeholder": "$(L,search)",
                "BarcodeScanner": true
              },
              "FilterProperty": "FldLogsCtnPackgStsCode",
              "PickerItems": {
                                "DisplayValue": "{{#Property:CtnPkgPackingStatus}} - {{#Property:CtnPkgPackingStatusText}}",
                                "ReturnValue": "{CtnPkgPackingStatus}",
                                "Target": {
                                    "EntitySet": "FldLogsCtnPkgPackingStatuses",
                                    "Service": "/SAPAssetManager/Services/AssetManager.service",
                                    "QueryOptions": "$orderby=CtnPkgPackingStatus"
                                }
                            },
              "OnValueChange": "/SAPAssetManager/Rules/Filter/RedrawFilterToolbar.js"
            },
           {
              "_Name": "VoyageAsignStatusFilter",
              "_Type": "Control.Type.FormCell.ListPicker",
              "Caption": "$(L,fld_voyage_assignment_status)",
              "AllowMultipleSelection": false,
              "IsPickerDismissedOnSelection": true,
              "IsSearchCancelledAfterSelection": true,
              "Search": {
                "Enabled": true,
                "Delay": 500,
                "MinimumCharacterThreshold": 3,
                "Placeholder": "$(L,search)",
                "BarcodeScanner": true
              },
              "FilterProperty": "FldLogsVoyAssgmtStatusText",
              "PickerItems": "/SAPAssetManager/Rules/FL/ReadyToPack/FLReadyToPackVoyageAsignStatusPickerItems.js",
              "OnValueChange": "/SAPAssetManager/Rules/Filter/RedrawFilterToolbar.js"
            },
            {
              "_Type": "Control.Type.FormCell.SimpleProperty",
              "_Name": "ContainerId",
              "AlternateInput": "Barcode",
              "FilterProperty": "FldLogsContainerId",
              "Caption": "$(L,fl_container_id)"
            }
          ]
        }
      ],
      "_Name": "FormCellContainer",
      "_Type": "Control.Type.FormCellContainer"
    }
  ],
  "FioriToolbar": {
    "_Name": "OrderProductSearchFilterFioriToolbar",
    "_Type": "Control.Type.FioriToolbar",
    "Items": [
      {
        "_Type": "Fragment",
        "Fragment": "/SAPAssetManager/Pages/Fragments/Filter/ResetFilterToolbarItem.fragment"
      }
    ]
  },
  "_Type": "Page",
  "_Name": "FLReadyToPackFilter"
}
