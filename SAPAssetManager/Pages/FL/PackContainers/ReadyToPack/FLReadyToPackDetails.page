            },
            {
                "_Name": "VoyagesListTab",
                "Caption": "$(L, fld_voyages)",
                "PageToOpen" : "/SAPAssetManager/Pages/FL/PackContainers/ReadyToPack/ReadyToPackVoyagesPage.page",
