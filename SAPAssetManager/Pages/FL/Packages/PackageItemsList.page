{
"Caption": "$(L,fld_package_items)",
"ActionBar": {
		"Items": [
            {
                "Position": "Right",
                "Icon": "$(PLT,sap-icon://filter,sap-icon://filter, '' , sap-icon://filter)",
                "Caption": "/SAPAssetManager/Rules/Filter/FilterButtonCaption.js",
                "OnPress": "/SAPAssetManager/Actions/FL/Packages/PackagesFilterViewNav.action",
				"_Name": "FilterButton"
            },
			"/SAPAssetManager/Pages/Fragments/SyncButton.fragment"
		]
	},
"Controls": [
		{
			"Sections": [
				{
					"EmptySection": {
						"Caption": "$(L,no_fld_packages)"
					},
					"Header": {
						"Caption":"/SAPAssetManager/Rules/FL/PackageItems/PackageItemsListCaption.js",
						"UseTopPadding": false,
						"Items": [
							{
								"_Type": "Fragment",
								"Fragment": "/SAPAssetManager/Pages/Fragments/FL/ListCounterCaption.fragment",
								"Properties": {
									"Title": "/SAPAssetManager/Rules/FL/PackageItems/PackageItemsListCaption.js"
								}
							},
                            {
                                "_Type": "SectionHeaderItem.Type.Button",
                                "_Name": "SelectItems",
                                "Title": "$(L,select_items)",
                                "Position": "Right",
                                "Visible": true,
                                "Enabled": "/SAPAssetManager/Rules/FL/PackageItems/ListView/IsSelectItemsButtonActive.js",
                                "OnPress": "/SAPAssetManager/Rules/FL/SwitchSelect.js",
								"Styles": {
                                    "Button": "SelectItemsButtonLabelFontSize"
                                }
                            },
							{
                                "_Type": "SectionHeaderItem.Type.Button",
                                "_Name": "DeselectAll",
                                "Title": "$(L,deselect_all)",
                                "Position": "Right",
                                "Visible": false,
                                "OnPress": "/SAPAssetManager/Rules/FL/SwitchSelect.js",
								"Styles": {
                                    "Button": "SelectItemsButtonLabelFontSize"
                                }
                            },
                            {
                                "_Type": "SectionHeaderItem.Type.Button",
                                "_Name": "SelectAll",
                                "Title": "$(L,select_all)",
                                "Position": "Right",
                                "Visible": false,
                                "OnPress": "/SAPAssetManager/Rules/FL/SwitchSelect.js",
								"Styles": {
                                    "Button": "SelectItemsButtonLabelFontSize"
                                }
                            }
						]
					},
					"Selection": {
						"LongPressToEnable": "Multiple",
						"ExitOnLastDeselect": true,
						"Mode": "None"
					},
					"OnSelectionChanged": "/SAPAssetManager/Rules/FL/ContainerItems/OnDocumentSelectedOrUnselected.js",
					"ObjectCell": {
       				    "_Type": "Fragment",
        				"Fragment": "/SAPAssetManager/Pages/Fragments/FL/PackageItemsCell.fragment",
        				"Properties": {
          					"OnAccessoryButtonPress": "/SAPAssetManager/Actions/FL/Edit/EditFLDocumentPageNav.action",
							"AccessoryButtonIcon": "/SAPAssetManager/Rules/FL/ContainerItems/IsEditAllowed.js"
        				}
   					 },
					"Search": {
						"Enabled" : true,
                        "Delay": 500,
                        "MinimumCharacterThreshold": 3,
                        "Placeholder": "$(L,search)",
                        "BarcodeScanner": true
					},
					"Target": {
						"EntitySet": "FldLogsPackageItems",
						"QueryOptions":"/SAPAssetManager/Rules/FL/PackageItems/PackageItemsOnLoadQuery.js",
						"Service": "/SAPAssetManager/Services/AssetManager.service"
					},
					"_Name": "SectionObjectTable",
					"_Type": "Section.Type.ObjectTable",
					"DataSubscriptions": [
						"PackageItems"
					]
				}
			],
			"FilterFeedbackBar": {
				"_Type": "Control.Type.FilterFeedbackBar",
				"_Name": "FilterFeedback",
				"ShowAllFilters": true,
				"FastFilters": [
	        		{ 
						"_Name": "ItemsOpen",
						"_Type": "Control.Type.FastFilterItem",
						"FilterType": "Filter",
						"CustomQueryGroup": "PackageItemsList",
						"DisplayValue": "/SAPAssetManager/Rules/FL/PackageItems/PackageItemsFilterCaptionOpen.js",
						"ReturnValue": "/SAPAssetManager/Rules/FL/PackageItems/GetReturnValueforItemsOpen.js"
            		},
            		{	 
						"_Name": "ItemsReceived",
						"_Type": "Control.Type.FastFilterItem",
						"FilterType": "Filter",
						"CustomQueryGroup": "PackageItemsList",
						"DisplayValue": "/SAPAssetManager/Rules/FL/PackageItems/PackageItemsFilterCaptionReceived.js",
						"ReturnValue": "/SAPAssetManager/Rules/FL/PackageItems/GetReturnValueforItemsReceived.js"       
            		},
					{
						"_Name": "NotFoundPackageItems",
						"_Type": "Control.Type.FastFilterItem",
						"FilterType": "Filter",
						"CustomQueryGroup": "PackageItemsList",
						"DisplayValue": "/SAPAssetManager/Rules/FL/PackageItems/PackageItemsFilterCaptionNotFound.js",
						"ReturnValue": "/SAPAssetManager/Rules/FL/PackageItems/PackageItemsReturnValueFilterNotFound.js"
					}
				]
			},
			"Filters": "/SAPAssetManager/Rules/FL/PackageItems/GetPackageItemsPreSelectedFilter.js",
			"_Name": "SectionedTable",
			"_Type": "Control.Type.SectionedTable"
		}
],
		"FioriToolbar": {
        "_Name": "PackageItemsFioriToolbar",
        "_Type": "Control.Type.FioriToolbar",
        "Items": [
		   { 
			"_Type": "Fragment",
            "Fragment": "/SAPAssetManager/Pages/Fragments/FL/ReceiveAll.fragment"           
			},
			{ 
			"_Type": "Fragment",
            "Fragment": "/SAPAssetManager/Pages/Fragments/FL/ReceiveItem.fragment"           
			},
			{
			"_Type": "Fragment",
			"Fragment": "/SAPAssetManager/Pages/Fragments/FL/NotFoundItem.fragment"
			},
			{
			"_Type": "Fragment",
            "Fragment": "/SAPAssetManager/Pages/Fragments/FL/EditAll.fragment"
			}    
        ]
    },
	"DataSubscriptions" : [
    	"FldLogsPackageItems"
	],
	"_Name": "PackageItemsListPage",
	"_Type": "Page",
	"OnLoaded": "/SAPAssetManager/Rules/Filter/ListPageWithFilterOnLoaded.js",
	"OnReturning" :"/SAPAssetManager/Rules/FL/ContainerItems/RedrawFilter.js"
}
