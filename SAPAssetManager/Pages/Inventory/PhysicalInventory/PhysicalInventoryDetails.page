					"OnPress": "/SAPAssetManager/Actions/Inventory/PhysicalInventory/PhysicalInventoryFilterViewNav.action"
				}
			},
			"/SAPAssetManager/Pages/Fragments/SyncButton.fragment"
		]
	},
	"Controls": [
		{
			"Sections": [
				{
					"ObjectHeader": {
						"HeadlineText": "/SAPAssetManager/Rules/Inventory/PhysicalInventory/PhysicalInventoryDocAndDescription.js"
					},
					"_Name": "SectionObjectHeader0",
					"_Type": "Section.Type.ObjectHeader"
				}
			],
			"_Name": "SectionedTable1",
			"_Type": "Control.Type.SectionedTable"
		},
		{
			"Items": [{
