{
    "OnActivityBackPressed": "/SAPAssetManager/Rules/WorkOrders/CreateUpdate/CancelCheckOnBackNavAndroid.js",
    "ActionBar": {
        "Items": [
            {
                "Position": "Left",
                "SystemItem": "Cancel",
                "Caption": "$(L,cancel)",
                "OnPress": "/SAPAssetManager/Actions/Page/CancelPage.action"
            },
            {
                "Position": "Right",
                "SystemItem": "/SAPAssetManager/Rules/Filter/FilterSystemItem.js",
                "Text": "/SAPAssetManager/Rules/Filter/FilterText.js",
                "OnPress": "/SAPAssetManager/Rules/Filter/ApplyFilterAndSave.js"
            }
        ]
    },
    "Caption": "$(L,filter)",
    "Result": "/SAPAssetManager/Rules/Inventory/ProductionOrder/SearchFilter/ProductionOrderSearchFilterResults.js",
    "Controls": [
        {
            "Sections": [
                {
                    "Controls": [
                        {
                            "_Type": "Fragment",
                            "Fragment": "/SAPAssetManager/Pages/Fragments/Filter/MultiSortByFilter.fragment",
                            "Properties": {
                                "Items": [
                                    {
                                        "ReturnValue": "ItemNum",
                                        "DisplayValue": "$(L,item_number)"
                                    },
                                    {
                                        "ReturnValue": "StorageBin",
                                        "DisplayValue": "$(L,storage_bin)"
                                    }
                                ]
                            }
                        }
                    ]
                },
                {
                    "Caption": "$(L,filter_by)",
                    "Controls": [
                        {
                            "_Type": "Fragment",
                            "Fragment": "/SAPAssetManager/Pages/Fragments/Inventory/ListPicker.fragment",
                            "Properties": {
                                "_Name": "ItemNum",
                                "Caption": "$(L,item_number)",
                                "PickerItems": "/SAPAssetManager/Rules/Inventory/ProductionOrder/SearchFilter/ProductionOrderItemNumberPickerItems.js",
                                "FilterProperty": "ItemNum"
                            }
                        },
                        {
                            "_Type": "Fragment",
                            "Fragment": "/SAPAssetManager/Pages/Fragments/Inventory/ListPicker.fragment",
                            "Properties": {
                                "_Name": "MaterialNum",
                                "Caption": "$(L,material_number)",
                                "PickerItems": "/SAPAssetManager/Rules/Inventory/ProductionOrder/SearchFilter/ProductionOrderMaterialNumberPickerItems.js",
                                "FilterProperty": "MaterialNum"
                            }
                        },
                        {
                            "_Type": "Fragment",
                            "Fragment": "/SAPAssetManager/Pages/Fragments/Inventory/ListPicker.fragment",
                            "Properties": {
                                "_Name": "Batch",
                                "Caption": "$(L,batch_number)",
                                "PickerItems": "/SAPAssetManager/Rules/Inventory/ProductionOrder/SearchFilter/ProductionOrderBatchPickerItems.js",
                                "FilterProperty": "Batch"
                            }
                        },
                        {
                            "_Type": "Fragment",
                            "Fragment": "/SAPAssetManager/Pages/Fragments/Inventory/ListPicker.fragment",
                            "Properties": {
                                "_Name": "StorageBin",
                                "Caption": "$(L,storage_bin)",
                                "PickerItems": "/SAPAssetManager/Rules/Inventory/ProductionOrder/SearchFilter/ProductionOrderStorageBinPickerItems.js",
                                "FilterProperty": "StorageBin"
                            }
                        }
                    ]
                }
            ],
            "_Name": "FormCellContainer",
            "_Type": "Control.Type.FormCellContainer"
        }
    ],
    "FioriToolbar": {
        "_Name": "StockSearchFilterFioriToolbar",
        "_Type": "Control.Type.FioriToolbar",
        "Items": [
            {
                "_Type": "Fragment",
                "Fragment": "/SAPAssetManager/Pages/Fragments/Filter/ResetFilterToolbarItem.fragment"
            }
        ]
    },
    "_Type": "Page",
    "_Name": "ProductionOrderSearchFilterPage",
    "OnLoaded": "/SAPAssetManager/Rules/Filter/FilterOnLoaded.js"
}