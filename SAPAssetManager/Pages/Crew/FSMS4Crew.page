{
	"_Name": "FSMS4CrewPage",
	"_Type": "Page",
	"OnLoaded": "/SAPAssetManager/Rules/Filter/ListPageWithFilterOnLoaded.js",
	"Caption": "/SAPAssetManager/Rules/Crew/FSM/FSMCrewCaption.js",
	"ActionBar": {
		"Items": [
			{
				"_Name": "Filter",
				"Position": "Right",
				"Icon": "sap-icon://filter",
				"Caption": "/SAPAssetManager/Rules/Filter/FilterButtonCaption.js",
				"OnPress": "/SAPAssetManager/Actions/Crew/FSMS4CrewFilter.action"
			}
		]
	},
	"Controls": [
		{
			"_Type" : "Control.Type.SectionedTable",
			"_Name": "SectionedTable",
			"DataSubscriptions": ["CrewLists", "CrewListItems"],
			"Filters": "/SAPAssetManager/Rules/Crew/FSM/FSMCrewFilters.js",
			"FilterFeedbackBar": {
				"_Type": "Control.Type.FilterFeedbackBar",
				"_Name": "FilterFeedback",
				"ShowAllFilters": true,
				"FastFilters": [
					{ 
						"_Name": "ActiveFilter",
						"_Type": "Control.Type.FastFilterItem",
						"FilterType": "Filter",
						"FilterProperty": "Status",
						"CustomQueryGroup": "Status",
						"DisplayValue": "$(L,wcm_active)",
						"ReturnValue": "ACTIVE"
					},
					{ 
						"_Name": "InactiveFilter",
						"_Type": "Control.Type.FastFilterItem",
						"FilterType": "Filter",
						"FilterProperty": "Status",
						"CustomQueryGroup": "Status",
						"DisplayValue": "$(L,wcm_inactive)",
						"ReturnValue": "INACTIVE"
					}
				]
			},
			"Sections": [
				{
					"Search": {
						"Enabled": true,
						"Delay": 500,
						"MinimumCharacterThreshold": 3,
						"Placeholder": "$(L,search)",
						"BarcodeScanner": false,
						"Mode": "Persistent"
					},
					"ObjectCell": {
						"Title": "{Name}",
						"Subhead": "{Description}",
						"StatusText": "/SAPAssetManager/Rules/Crew/FSM/FSMCrewStatusText.js",
						"Description": "/SAPAssetManager/Rules/Crew/FSM/S4/FSMS4CrewDescriptionText.js",
						"AccessoryType": "DisclosureIndicator",
						"OnPress": "/SAPAssetManager/Actions/Crew/FSMCrewDetailsNav.action",
						"Styles": {
							"StatusText": "/SAPAssetManager/Rules/Crew/FSM/FSMCrewStatusTextStyles.js"
						}
					},
					"Header": {
						"UseTopPadding": false
					},
					"EmptySection": {
					  "Caption": "$(L,no_crew_available)"
					},
					"Target": {
						"EntitySet": "CrewLists",
						"Service": "/SAPAssetManager/Services/AssetManager.service",
						"QueryOptions": "/SAPAssetManager/Rules/Crew/FSM/FSMCrewQueryOptions.js"
					},
					"_Name": "S4CrewTable",
					"_Type": "Section.Type.ObjectTable"
				}
			]
		}
	]
}