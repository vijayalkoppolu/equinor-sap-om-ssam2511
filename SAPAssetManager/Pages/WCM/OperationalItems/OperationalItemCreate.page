{
	"Caption": "{pageCaption}",
	"OnActivityBackPressed": "/SAPAssetManager/Rules/WCM/OperationalItems/Create/AndroidBackButtonPressed.js",
	"OnBackButtonPressed": "/SAPAssetManager/Rules/WCM/OperationalItems/Create/BackPressed.js",
	"ActionBar": {
		"Items": [
			{
				"Position": "Left",
				"SystemItem": "Cancel",
				"Caption": "$(L,cancel)",
				"Visible": "{showCancelButton}",
				"OnPress": "/SAPAssetManager/Rules/WCM/OperationalItems/Create/CancelPressed.js"
			},
			{
				"Position": "Right",
				"SystemItem": "$(PLT,'Done','', '','Done')",
				"Text": "/SAPAssetManager/Rules/Common/Platform/DoneText.js",
				"OnPress": "/SAPAssetManager/Rules/WCM/OperationalItems/Create/OperationalItemCreate.js"
			}
		]
	},
	"Controls": [
		{
			"Sections": [
				{
					"_Type":"Section.Type.FormCell",
					"_Name": "FormCellSection",
					"Controls": [
						{
							"_Type": "Control.Type.FormCell.SegmentedControl",
							"_Name": "ItemCategoryControl",
							"Caption": "$(L,item_category) *",
							"Segments": "/SAPAssetManager/Rules/WCM/OperationalItems/Create/ItemCategoryValues.js",
							"OnValueChange": "/SAPAssetManager/Rules/WCM/OperationalItems/Create/ItemCategoryControlChanged.js",
							"IsVisible": true,
							"Value": "$(DV,#Property:item/#Property:ItemCategoryCC, '')"
						},
						{
							"_Type": "Control.Type.FormCell.ListPicker",
							"_Name": "TechnicalObjectControl",
							"Caption": "$(L,wcm_technical_object) *",
							"IsVisible": "#Property:codeBehind/#Property:isPropFilledFlocOrEquipmentType/#Property:ItemCategoryCC|TechObject",
							"AllowMultipleSelection": false,
							"IsPickerDismissedOnSelection": true,
							"IsSearchCancelledAfterSelection": true,
							"Search": {
								"Enabled": true,
								"Delay": 500,
								"MinimumCharacterThreshold": 3,
								"Placeholder": "$(L,search)",
								"BarcodeScanner": true
							},
							"PickerItems": {
								"DisplayValue": "/SAPAssetManager/Rules/WCM/OperationalItems/Create/TechnicalObjectControlDisplayValue.js",
								"ReturnValue": "/SAPAssetManager/Rules/WCM/OperationalItems/Create/TechnicalObjectControlReturnValue.js",
								"Target": {
									"EntitySet": "#Property:codeBehind/#Property:TechnicalObjectControlPickerItems/#Property:EntitySet",
									"Service": "/SAPAssetManager/Services/AssetManager.service",
									"QueryOptions": "#Property:codeBehind/#Property:TechnicalObjectControlPickerItems/#Property:QueryOptions"
								}
							},
							"OnValueChange": "/SAPAssetManager/Rules/WCM/OperationalItems/Create/TechnicalObjectControlChanged.js",
							"Value": "$(DV,#Property:item/#Property:TechObject, '')"
						},
						{
							"_Type": "Control.Type.FormCell.SimpleProperty",
							"_Name": "CommentDescriptionControl",
							"Caption": "$(L,description) *",
							"PlaceHolder": "$(L,description)",
							"IsVisible": "#Property:codeBehind/#Property:isPropFilledCommentType/#Property:ItemCategoryCC|ShortText",
							"OnValueChange": "/SAPAssetManager/Rules/WCM/OperationalItems/Create/ValidateControlOnChange.js",
							"Value": "$(DV,#Property:item/#Property:ShortText, '')"
						},
						{
							"_Type": "Control.Type.FormCell.SimpleProperty",
							"_Name": "CommentTaggingSequenceNote",
							"Caption": "$(L,tag_sequence)",
							"PlaceHolder": "$(L,tag_sequence)",
							"IsVisible": "#Property:codeBehind/#Property:isPropFilledCommentType/#Property:ItemCategoryCC|TagSequence",
							"KeyboardType": "Number",
							"OnValueChange": "/SAPAssetManager/Rules/WCM/OperationalItems/Create/ValidateControlOnChange.js",
							"Value": "$(DV,#Property:item/#Property:TagSequence, '')"
						},
						{
							"_Type": "Control.Type.FormCell.SimpleProperty",
							"_Name": "CommentUntaggingSequenceNote",
							"Caption": "$(L,untag_sequence)",
							"PlaceHolder": "$(L,untag_sequence)",
							"IsVisible": "#Property:codeBehind/#Property:isPropFilledCommentType/#Property:ItemCategoryCC|UntSequence",
							"KeyboardType": "Number",
							"OnValueChange": "/SAPAssetManager/Rules/WCM/OperationalItems/Create/ValidateControlOnChange.js",
							"Value": "$(DV,#Property:item/#Property:UntSequence, '')"
						},
						{
							"_Type": "Control.Type.FormCell.SimpleProperty",
							"_Name": "WOMasterDataTechnicalObjectNameControl",
							"Caption": "$(L,wcm_technical_object_name) *",
							"PlaceHolder": "$(L,wcm_technical_object_name)",
							"IsVisible": "#Property:codeBehind/#Property:isPropFilledWOMasterDataType/#Property:ItemCategoryCC|TechObject",
							"OnValueChange": "/SAPAssetManager/Rules/WCM/OperationalItems/Create/WOMasterDataTechnicalObjectNameControlChange.js",
							"Value": "$(DV,#Property:item/#Property:TechObject, '')"
						},
						{
							"_Type": "Control.Type.FormCell.SimpleProperty",
							"_Name": "SwitchingLocationControl",
							"Caption": "$(L,switching_location)",
							"PlaceHolder": "$(L,switching_location)",
							"IsVisible": "#Property:codeBehind/#Property:isPropFilledNotCommentType/#Property:ItemCategoryCC|SwitchingLoc",
							"OnValueChange": "/SAPAssetManager/Rules/WCM/OperationalItems/Create/ValidateControlOnChange.js",
							"Value": "$(DV,#Property:item/#Property:SwitchingLoc, '')"
						},
						{
							"_Type": "Control.Type.FormCell.SimpleProperty",
							"_Name": "PhysicalLocationControl",
							"Caption": "$(L,physical_location)",
							"PlaceHolder": "$(L,physical_location)",
							"IsVisible": "#Property:codeBehind/#Property:isPropFilledNotCommentType/#Property:ItemCategoryCC|Location",
							"OnValueChange": "/SAPAssetManager/Rules/WCM/OperationalItems/Create/ValidateControlOnChange.js",
							"Value": "$(DV,#Property:item/#Property:Location, '')"
						}
					]
				},
				{
					"_Type":"Section.Type.FormCell",
					"_Name": "OperationalGroupFormCellSection",
					"Visible": "#Property:codeBehind/#Property:isPropFilledNotCommentType/#Property:ItemCategoryCC|TechObject|OpGroup",
					"Controls": [
						{
							"_Type": "Control.Type.FormCell.SegmentedControl",
							"_Name": "OperationalGroupControl",
							"Caption": "$(L,operational_group) *",
							"Segments": "/SAPAssetManager/Rules/WCM/OperationalItems/Create/OperationalGroupControlValues.js",
							"OnValueChange": "/SAPAssetManager/Rules/WCM/OperationalItems/Create/OperationalGroupControlChanged.js",
							"Value": "$(DV,#Property:item/#Property:OpGroup, '')",
							"IsVisible": true
						}
					]
				},
				{
					"_Type":"Section.Type.FormCell",
					"_Name": "TaggingConditionSection",
					"Visible": "#Property:codeBehind/#Property:isPropFilledNotCommentType/#Property:OpGroup|TaggingCond|TaggingType|TagSequence|TaggingComment|TagRequired|LockNumber",
					"Controls": [
						{
							"_Type": "Control.Type.FormCell.ListPicker",
							"_Name": "TaggingConditionPicker",
							"Caption": "$(L,tag_condition) *",
							"AllowMultipleSelection": false,
							"AllowEmptySelection": false,
							"IsPickerDismissedOnSelection": true,
							"IsVisible": true,
							"PickerItems": "/SAPAssetManager/Rules/WCM/OperationalItems/Create/TaggingConditionPickerItems.js",
							"OnValueChange": "/SAPAssetManager/Rules/WCM/OperationalItems/Create/TaggingConditionPickerChanged.js",
							"Value": "$(DV,#Property:item/#Property:TaggingCond, '')"
						},
						{
							"_Type": "Control.Type.FormCell.ListPicker",
							"_Name": "TaggingTypeControl",
							"Caption": "$(L, tag_type)",
							"AllowMultipleSelection": false,
							"AllowEmptySelection": false,
							"IsPickerDismissedOnSelection": true,
							"IsEditable": false,
							"IsVisible": true,
							"PickerItems": "/SAPAssetManager/Rules/WCM/OperationalItems/Create/TagUntagConditionTypeValues.js",
							"Value": "$(DV,#Property:item/#Property:TaggingType, '')"
						},
						{
							"_Type": "Control.Type.FormCell.SimpleProperty",
							"_Name": "TaggingSequenceNote",
							"Caption": "$(L,tag_sequence)",
							"PlaceHolder": "$(L,tag_sequence)",
							"KeyboardType": "Number",
							"IsVisible": true,
							"OnValueChange": "/SAPAssetManager/Rules/WCM/OperationalItems/Create/ValidateControlOnChange.js",
							"Value": "$(DV,#Property:item/#Property:TagSequence, '')"
						},
						{
							"_Type": "Control.Type.FormCell.Note",
							"_Name": "TaggingCommentNote",
							"Caption": "$(L,tagging_comment)",
							"MinNumberOfLines": 2,
							"PlaceHolder": "$(L,tagging_comment)",
							"IsVisible": true,
							"OnValueChange": "/SAPAssetManager/Rules/WCM/OperationalItems/Create/ValidateControlOnChange.js",
							"Value": "$(DV,#Property:item/#Property:TaggingComment, '')"
						},
						{
							"_Type": "Control.Type.FormCell.Switch",
							"_Name": "TagRequiredSlider",
							"Caption": "$(L,tag_required)",
							"IsEditable": false,
							"IsVisible": true,
							"Value": "#Property:codeBehind/#Property:convertToBool/#Property:TagRequired"
						},
						{
							"_Type": "Control.Type.FormCell.SimpleProperty",
							"_Name": "LockNote",
							"Caption": "$(L,lock)",
							"PlaceHolder": "$(L,lock)",
							"IsVisible": true,
							"OnValueChange": "/SAPAssetManager/Rules/WCM/OperationalItems/Create/ValidateControlOnChange.js",
							"Value": "$(DV,#Property:item/#Property:LockNumber, '')"
						}
					]
				},
				{
					"_Type":"Section.Type.FormCell",
					"_Name": "UntaggingConditionSection",
					"Visible": "#Property:codeBehind/#Property:isPropFilledNotCommentType/#Property:UntagCond|UntaggingType|UntSequence|UntagComment",
					"Controls": [
						{
							"_Type": "Control.Type.FormCell.ListPicker",
							"_Name": "UntaggingConditionPicker",
							"Caption": "$(L,untag_condition)",
							"AllowMultipleSelection": false,
							"AllowEmptySelection": false,
							"IsPickerDismissedOnSelection": true,
							"IsEditable": false,
							"PickerItems": [],
							"IsVisible": true,
							"Value": "$(DV,#Property:item/#Property:UntagCond, '')"
						},
						{
							"_Type": "Control.Type.FormCell.ListPicker",
							"_Name": "UntaggingTypeControl",
							"Caption": "$(L, untag_type)",
							"AllowMultipleSelection": false,
							"AllowEmptySelection": false,
							"IsPickerDismissedOnSelection": true,
							"IsEditable": false,
							"IsVisible": true,
							"PickerItems": "/SAPAssetManager/Rules/WCM/OperationalItems/Create/TagUntagConditionTypeValues.js",
							"Value": "$(DV,#Property:item/#Property:UntaggingType, '')"
						},
						{
							"_Type": "Control.Type.FormCell.SimpleProperty",
							"_Name": "UntaggingSequenceNote",
							"Caption": "$(L,untag_sequence)",
							"PlaceHolder": "$(L,untag_sequence)",
							"KeyboardType": "Number",
							"IsVisible": true,
							"OnValueChange": "/SAPAssetManager/Rules/WCM/OperationalItems/Create/ValidateControlOnChange.js",
							"Value": "$(DV,#Property:item/#Property:UntSequence, '')"
						},
						{
							"_Type": "Control.Type.FormCell.Note",
							"_Name": "UntaggingCommentNote",
							"Caption": "$(L,untagging_comment)",
							"PlaceHolder": "$(L,untagging_comment)",
							"MinNumberOfLines": 2,
							"IsVisible": true,
							"OnValueChange": "/SAPAssetManager/Rules/WCM/OperationalItems/Create/ValidateControlOnChange.js",
							"Value": "$(DV,#Property:item/#Property:UntagComment, '')"
						}
					]
				},
				{
					"_Type":"Section.Type.FormCell",
					"_Name": "BlockingSection",
					"Visible": "#Property:codeBehind/#Property:isPropFilledNotCommentType/#Property:BlockingType|PhysBlocking",
					"Controls": [
						{
							"_Type": "Control.Type.FormCell.ListPicker",
							"_Name": "BlockingTypePicker",
							"Caption": "$(L,blocking_type)",
							"AllowMultipleSelection": false,
							"AllowEmptySelection": true,
							"IsPickerDismissedOnSelection": true,
							"IsVisible": true,
							"PickerItems": {
								"DisplayValue": "{BlockingType} - {BlockingTypeText}",
								"ReturnValue": "{BlockingType}",
								"Target": {
									"EntitySet": "WCMPhysicalBlockingTypes",
									"Service": "/SAPAssetManager/Services/AssetManager.service",
									"QueryOptions": "$filter=PlanningPlant eq '{{#Property:PlanningPlant}}'&$orderby=BlockingType"
								}
							},
							"Value": "$(DV,#Property:item/#Property:BlockingType, '')"
						},
						{
							"_Type": "Control.Type.FormCell.Switch",
							"_Name": "PhysicalBlockingRequiredSwitch",
							"Caption": "$(L,phys_blocking_req)",
							"IsVisible": true,
							"Value": "#Property:codeBehind/#Property:convertToBool/#Property:PhysBlocking"
						}
					]
				},
                {
                    "_Type": "Section.Type.FormCell",
                    "_Name": "DiscardButtonSection",
                    "Controls": [
                        {
                            "Title": "$(L,discard)",
                            "OnPress": "/SAPAssetManager/Rules/WCM/OperationalItems/Create/OperationalItemDiscardPressed.js",
                            "IsVisible": "{showDiscardButton}",
                            "_Type": "Control.Type.FormCell.Button",
                            "ButtonType": "Text",
                            "Semantic": "Tint",
                            "_Name": "DiscardButton",
                            "TextAlignment": "/SAPAssetManager/Rules/Common/Platform/ModalButtonAlign.js"
                        }
                    ]
                }
			],
			"_Name": "FormCellContainer",
			"_Type": "Control.Type.SectionedTable"
		}
	],
	"_Name": "OperationalItemCreate",
	"_Type": "Page"
}