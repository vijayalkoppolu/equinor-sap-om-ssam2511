{
    "Visible": "/SAPAssetManager/Rules/WorkOrders/Meter/ShowMeterSectionDetails.js",
    "Header": {
        "UseTopPadding": false,
        "Items": [
            {
                "_Type": "SectionHeaderItem.Type.Label",
                "_Name": "MeterLabel",
                "Title": "$(L,meters)",
                "Position": "Left"
            },
            {
                "_Type": "SectionHeaderItem.Type.Button",
                "_Name": "MeterAction",
